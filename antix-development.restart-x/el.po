#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:18
#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:20
msgid "Restart X Windows"
msgstr "Επανεκκίνηση του διαχειριστή παραθύρων X Windows"

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:24
msgid "Pause between stopping and starting."
msgstr "Παύση ανάμεσα στη διακοπή και την εκκίνηση."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:25
msgid "DO NOT USE."
msgstr "ΝΑ ΜΗΝ ΧΡΗΣΙΜΟΠΟΙΕΙΤΑΙ."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:49
msgid "The default display manager was not found."
msgstr "Ο προεπιλεγμένος διαχειριστής παραθύρων δεν βρέθηκε."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:51
msgid "Will search for any running display manager."
msgstr "Θα γίνει αναζήτηση για οποιονδήποτε διαχειριστή παραθύρων εντοπισθεί."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:54
msgid "No display manager was found."
msgstr "Δεν βρέθηκε κανένας διαχειριστής παραθύρων."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:55
msgid "Cannot restart X."
msgstr "Αδύνατη η επανεκκίνηση του Χ."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:59
msgid "Found display manager: %s"
msgstr "Βρέθηκε ο διαχειριστής παραθύρων: %s"

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:61
msgid "Do you want to restart X-windows now?  "
msgstr "Θέλετε να επανεκκινήσετε το X-windows τώρα;"

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/restart-X:63
msgid "(There will be a %s second pause before X-windows starts) "
msgstr "(Θα υπάρξει μία %s δεύτερη παύση πριν αρχίσει ο X-windows) "
