msgid "All"
msgstr "Всех"

#: lib/antiX-common.sh:1273
msgid "Mounted"
msgstr "Смонтированных"

#: lib/antiX-common.sh:1274
msgid "Unmounted"
msgstr "Отмонтированных"

#: lib/antiX-common.sh:1277
msgid "devices"
msgstr "устройств"

#: lib/antiX-common.sh:1289
msgid "OK"
msgstr "OK"

#: lib/antiX-common.sh:1291
msgid "Click on a device then hit %s"
msgstr "Кликните устройство, затем нажмите %s"

#: lib/antiX-common.sh:1298
msgid "Device"
msgstr "Устройство"

#: lib/antiX-common.sh:1299
msgid "Mount"
