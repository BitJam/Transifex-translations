#: lib/antiX-common.sh:1298
msgid "Device"
msgstr "Dispositivo"

#: lib/antiX-common.sh:1299
msgid "Mount"
msgstr "Monta"

#: lib/antiX-common.sh:1300
msgid "Type"
msgstr "Tipo"

#: lib/antiX-common.sh:1301
msgid "Label"
msgstr "Etichetta"

#: lib/antiX-common.sh:1302
msgid "Size"
msgstr "Dimensione"

#: lib/antiX-common.sh:1303
msgid "Free"
