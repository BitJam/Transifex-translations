msgid "All"
msgstr "Tutti"

#: lib/antiX-common.sh:1273
msgid "Mounted"
msgstr "Montati"

#: lib/antiX-common.sh:1274
msgid "Unmounted"
msgstr "Smontati"

#: lib/antiX-common.sh:1277
