#: antix-system.sh:52
msgid "Locale"
msgstr "Locale"

#: antix-system.sh:53
msgid "Disk_Management"
msgstr "Διαχείριση Δίσκου"

#: antix-system.sh:54
msgid "Grub"
msgstr "Grub"

#: antix-system.sh:63
msgid "Locale options"
msgstr "Επιλογές locale"

#: antix-system.sh:65 antix-system.sh:119
msgid "Enable locale options"
msgstr "Ενεργοποίηση επιλογών locale"

#: antix-system.sh:75
msgid "Names"
msgstr "Ονόματα"

#: antix-system.sh:77
msgid "Enable name change"
msgstr "Ενεργοποίηση αλλαγής ονόματος"

#: antix-system.sh:91
msgid "Computer name"
msgstr "Όνομα υπολογιστή"

#: antix-system.sh:99
msgid "Computer domain"
msgstr "Δίκτυο στο οποίο ανήκει ο Υπολογιστής"

#: antix-system.sh:107
msgid "Samba work group"
msgstr "Ομάδα Εγρασίας Samba"

#: antix-system.sh:117
msgid "Locales"
msgstr "Locales"

#: antix-system.sh:135
msgid "Change language"
msgstr "Αλλαγή γλώσσας"

#: antix-system.sh:139
msgid "enable language select"
msgstr "ενεργοποίηση επιλογής γλώσσας"

#: antix-system.sh:145
msgid "select console locale"
msgstr "επιλογή γλώσσας τερματικού"

#: antix-system.sh:151
msgid "select keyboard map"
msgstr "επιλογή διάταξης πληκτρολογίου"

#: antix-system.sh:156
msgid "Change mouse order"
msgstr "Αλλαγή ενεργειών ποντικιού"

#: antix-system.sh:161
msgid "Leave default"
msgstr "Αφήστε την προεπιλογή"

#: antix-system.sh:162
msgid "Left hand"
msgstr "Αριστερό χέρι"

#: antix-system.sh:163
msgid "Right hand"
msgstr "Δεξί χέρι"

#: antix-system.sh:172
msgid "Disk management"
msgstr "Διαχείρισης Δίσκου"

#: antix-system.sh:174
msgid "Enable Disk Management"
msgstr "Ενεργοποίηση Διαχείρισης Δίσκου"

#: antix-system.sh:188 antix-system.sh:287
msgid "Options"
msgstr "Επιλογές"

#: antix-system.sh:190
msgid "Clear Package Cache"
msgstr "Εκκαθάριση cache των πακέτων"

#: antix-system.sh:196
msgid "Clear Logs"
msgstr "Εκκαθάριση αρχείων καταγραφής Logs"

#: antix-system.sh:202
msgid "Show basic disk usage"
msgstr "Εμφάνιση βασικής χρήσης δίσκου"

#: antix-system.sh:208
msgid "Run partition manager"
msgstr "Εκκίνηση του διαχειριστή κατατμήσεων"

#: antix-system.sh:214
msgid "Run antiX to USB"
msgstr "Τρέξε το antiX στο USB"

#: antix-system.sh:224
msgid "Repair grub"
msgstr "Επισκευή του GRUB"

#: antix-system.sh:226
msgid "Enable grub repair"
msgstr "Ενεργοποίηση της επισκευής του GRUB"

#: antix-system.sh:240
msgid "Where to install"
msgstr "Που να γίνει εγκατάσταση"

#: antix-system.sh:242
msgid "Install on MBR"
msgstr "Εγκατάσταση στο MBR"

#: antix-system.sh:248
msgid "Install on root partition"
msgstr "Εγκατάσταση στην κατάτμηση συστήματος root"

#: antix-system.sh:256
msgid "Select system boot disk"
msgstr "Επιλογή δίσκου εκκίνησης συστήματος"

#: antix-system.sh:261 antix-system.sh:274
msgid "None selected"
msgstr "Δεν επιλέχθηκε τίποτε"

#: antix-system.sh:269
msgid "Select root partition"
msgstr "Επιλέξτε κατάτμηση συστήματος root"

#: antix-system.sh:289
msgid "Update grub menu"
msgstr "Ενημέρωση του μενού του GRUB"

#: antix-system.sh:345 antix-system.sh:379
msgid "Choose your locale for antiX applications"
msgstr "Επιλέξτε τις τοπικές ρυθμίσεις γλώσσας"

#: antix-system.sh:347
msgid "Locale chosen, antix application locale left default"
msgstr "Επιλέχθηκαν οι τοπικές ρυθμίσεις γλώσσας, παρέμειναν οι προεπιλογές για τις ρυθμίσεις γλώσσας των εφαρμογώνμ του antiX"

#: antix-system.sh:373
#, sh-format
msgid "Language has changed to $LOCALE"
msgstr "Η γλώσσα άλλαξε στο $LOCALE"

#: antix-system.sh:381
msgid "Keymap left default"
msgstr "Η διάταξη πληκτρολογίου παρέμεινε στη αρχική προεπιλογή"

#: antix-system.sh:397
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT)"
msgstr "Το πληκτρολόγιο ρυθμίσθηκε στο $KEYMAP. Επανεκκινείστε και χρησιμοποιήστε τη διάταξη πληκτρολογίου μέσω του ALT + SHIFT + (LEFT / RIGHT)"

#: antix-system.sh:401
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT). Mouse layout set to $MOUSE"
msgstr "Το πληκτρολόγιο ρυθμίσθηκε στο $KEYMAP. Επανεκκινείστε και χρησιμοποιήστε τη διάταξη πληκτρολογίου μέσω του ALT + SHIFT + (LEFT / RIGHT). Η διάταξη του ποντικιού ρυθμίσθηκε στο $MOUSE"

#: antix-system.sh:425 antix-system.sh:444
msgid "Need to add some valid information to change names"
msgstr "Πρέπει να προστεθούν μερικές έγκυρες πληροφορίες για να αλλαχθούν τα ονόματα."

#: antix-system.sh:429
#, sh-format
msgid "Samba workgroup has been changed from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Η Ομάδα Εργασίας Samba άλλαξε από το $OLDWORKGROUP σε $NEWWORKGROUP"

#: antix-system.sh:434
#, sh-format
msgid "Domain name has been changed from $OLDDOMAIN to $DOMAIN"
msgstr "Το Όνομα Χώρου άλλαξε από $OLDDOMAIN σε $DOMAIN"

#: antix-system.sh:437
#, sh-format
msgid ""
"Domain name has been changed from $OLDDOMAIN to $DOMAIN and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Το Όνομα Χώρου άλλαξε από $OLDDOMAIN σε $DOMAIN και η Ομάδα Εργασίας Samba άλλαξε από το $OLDWORKGROUP σε $NEWWORKGROUP"

#: antix-system.sh:448
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Το όνομα υπολογιστή άλλαξε από $OLDNAME σε $NAME και η Ομάδα Εργασίας Samba άλλαξε από το $OLDWORKGROUP σε $NEWWORKGROUP"

#: antix-system.sh:453
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Domain name from "
"$OLDDOMAIN to $DOMAIN"
msgstr "Το όνομα υπολογιστή άλλαξε από $OLDNAME σε $NAME και το Όνομα Χώρου από $OLDDOMAIN σε $DOMAIN"

#: antix-system.sh:456
#, sh-format
msgid ""
"Computer name name has been changed from $OLDNAME to $NAME, Domain name from"
" $OLDDOMAIN to $DOMAIN and Samba workgroup from $OLDWORKGROUP to "
"$NEWWORKGROUP"
msgstr "Το όνομα υπολογιστή άλλαξε από $OLDNAME σε $NAME, το Όνομα Χώρου από $OLDDOMAIN σε $DOMAIN και η Ομάδα Εργασίας Samba από $OLDWORKGROUP σε $NEWWORKGROUP"

#: antix-system.sh:466
msgid "Need to add some valid information to change locale options"
msgstr "Πρέπει να προστεθούν μερικές έγκυρες πληροφορίες για να αλλάξουν οι επιλογές στο locale"

#: antix-system.sh:470 antix-system.sh:482 antix-system.sh:496
#: antix-system.sh:508
#, sh-format
msgid "Mouse Layout set to $MOUSE"
msgstr "Η διάταξη Ποντικιού ρυθμίσθηκε στο $MOUSE"

#: antix-system.sh:523
msgid "Impossible error, radio buttons require at least one choice"
msgstr "Αδύνατο Σφάλμα, τα κουμπιά Ράδιο απαιτούν τουλάχιστον μία επιλογή"
