#: antix-system.sh:52
msgid "Locale"
msgstr "Lingua"

#: antix-system.sh:53
msgid "Disk_Management"
msgstr "Gestione_disco"

#: antix-system.sh:54
msgid "Grub"
msgstr "Grub"

#: antix-system.sh:63
msgid "Locale options"
msgstr "Opzioni relative alla lingua"

#: antix-system.sh:65 antix-system.sh:119
msgid "Enable locale options"
msgstr "Abilita le opzioni relative alla lingua"

#: antix-system.sh:75
msgid "Names"
msgstr "Nomi"

#: antix-system.sh:77
msgid "Enable name change"
msgstr "Abilita il cambiamento del nome"

#: antix-system.sh:91
msgid "Computer name"
msgstr "Nome del computer"

#: antix-system.sh:99
msgid "Computer domain"
msgstr "Dominio del computer"

#: antix-system.sh:107
msgid "Samba work group"
msgstr "Gruppo di lavoro Samba"

#: antix-system.sh:117
msgid "Locales"
msgstr "Lingue"

#: antix-system.sh:135
msgid "Change language"
msgstr "Cambia la lingua"

#: antix-system.sh:139
msgid "enable language select"
msgstr "abilita la selezione della lingua"

#: antix-system.sh:145
msgid "select console locale"
msgstr "seleziona la lingua della console"

#: antix-system.sh:151
msgid "select keyboard map"
msgstr "seleziona la mappatura della tastiera"

#: antix-system.sh:156
msgid "Change mouse order"
msgstr "Cambia l'ordine del mouse"

#: antix-system.sh:161
msgid "Leave default"
msgstr "Lascia le impostazioni predefinite"

#: antix-system.sh:162
msgid "Left hand"
msgstr "Mano sinistra"

#: antix-system.sh:163
msgid "Right hand"
msgstr "Mano destra"

#: antix-system.sh:172
msgid "Disk management"
msgstr "Gestione del disco"

#: antix-system.sh:174
msgid "Enable Disk Management"
msgstr "Abilita la Gestione del disco"

#: antix-system.sh:188 antix-system.sh:287
msgid "Options"
msgstr "Opzioni"

#: antix-system.sh:190
msgid "Clear Package Cache"
msgstr "Pulisci la cache dei pacchetti"

#: antix-system.sh:196
msgid "Clear Logs"
msgstr "Pulisci i Logs"

#: antix-system.sh:202
msgid "Show basic disk usage"
msgstr "Mostra l'utilizzo di base del disco"

#: antix-system.sh:208
msgid "Run partition manager"
msgstr "Avvia il gestore delle partizioni"

#: antix-system.sh:214
msgid "Run antiX to USB"
msgstr "Esegui antiX da USB"

#: antix-system.sh:224
msgid "Repair grub"
msgstr "Ripara grub"

#: antix-system.sh:226
msgid "Enable grub repair"
msgstr "Abilita la riparazione di grub"

#: antix-system.sh:240
msgid "Where to install"
msgstr "Dove installare"

#: antix-system.sh:242
msgid "Install on MBR"
msgstr "Installa su MBR"

#: antix-system.sh:248
msgid "Install on root partition"
msgstr "Installa nella partizione root"

#: antix-system.sh:256
msgid "Select system boot disk"
msgstr "Seleziona il disco di avvio del sistema"

#: antix-system.sh:261 antix-system.sh:274
msgid "None selected"
msgstr "Nessuno selezionato"

#: antix-system.sh:269
msgid "Select root partition"
msgstr "Seleziona la partizione di grub"

#: antix-system.sh:289
msgid "Update grub menu"
msgstr "Aggiorna il menù di grub"

#: antix-system.sh:345 antix-system.sh:379
msgid "Choose your locale for antiX applications"
msgstr "Scegli la tua lingua per le applicazioni di antiX"

#: antix-system.sh:347
msgid "Locale chosen, antix application locale left default"
msgstr "Liingua scelta, lingua dell'applicazione di antiX lasciata predefinita"

#: antix-system.sh:373
#, sh-format
msgid "Language has changed to $LOCALE"
msgstr "La lingua è stata cambiata in $LOCALE"

#: antix-system.sh:381
msgid "Keymap left default"
msgstr "Mappatura della tastiera lasciata predefinita"

#: antix-system.sh:397
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT)"
msgstr "Tastiera impostata su $KEYMAP, riavvia e utilizza la mappatura della tastiera con ALT + SHIFT + (SINISTRA / DESTRA)"

#: antix-system.sh:401
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT). Mouse layout set to $MOUSE"
msgstr "Tastiera impostata su $KEYMAP, riavvia e utilizza la mappatura della tastiera con ALT + SHIFT + (SINISTRA / DESTRA) Il layout del mouse è impostato su $MOUSE"

#: antix-system.sh:425 antix-system.sh:444
msgid "Need to add some valid information to change names"
msgstr "Bisogna aggiungere delle informazioni valide per cambiare i nomi"

#: antix-system.sh:429
#, sh-format
msgid "Samba workgroup has been changed from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Il gruppo di lavoro Samba è stato modificato da $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:434
#, sh-format
msgid "Domain name has been changed from $OLDDOMAIN to $DOMAIN"
msgstr "Il nome del dominio è stato modificato da $OLDDOMAIN a $DOMAIN"

#: antix-system.sh:437
#, sh-format
msgid ""
"Domain name has been changed from $OLDDOMAIN to $DOMAIN and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Il nome del dominio è stato modificato da $OLDDOMAIN a $DOMAIN e il gruppo di lavoro Samba da $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:448
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Il nome del computer è stato modificato da $OLDNAME a $NAME e il gruppo di lavoro Samba da $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:453
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Domain name from "
"$OLDDOMAIN to $DOMAIN"
msgstr "Il nome del computer è stato modificato da $OLDNAME a $NAME e il nome del dominio da $OLDDOMAIN a $DOMAIN"

#: antix-system.sh:456
#, sh-format
msgid ""
"Computer name name has been changed from $OLDNAME to $NAME, Domain name from"
" $OLDDOMAIN to $DOMAIN and Samba workgroup from $OLDWORKGROUP to "
"$NEWWORKGROUP"
msgstr "Il nome del computer è stato modificato da $OLDNAME a $NAME , il nome del dominio da $OLDDOMAIN a $DOMAIN e il gruppo di lavoro Samba da $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:466
msgid "Need to add some valid information to change locale options"
msgstr "E' necessario aggiungere delle informazioni valide per cambiare le opzioni di lingua locale."

#: antix-system.sh:470 antix-system.sh:482 antix-system.sh:496
#: antix-system.sh:508
#, sh-format
msgid "Mouse Layout set to $MOUSE"
msgstr "Layout del Mouse impostato su $MOUSE"

#: antix-system.sh:523
msgid "Impossible error, radio buttons require at least one choice"
msgstr "Errore, almeno una scelta deve essere spuntata"
