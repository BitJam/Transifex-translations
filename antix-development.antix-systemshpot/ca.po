#: antix-system.sh:52
msgid "Locale"
msgstr "Localització"

#: antix-system.sh:53
msgid "Disk_Management"
msgstr "Gestió de disc"

#: antix-system.sh:54
msgid "Grub"
msgstr "Grub"

#: antix-system.sh:63
msgid "Locale options"
msgstr "Opcions de localització"

#: antix-system.sh:65 antix-system.sh:119
msgid "Enable locale options"
msgstr "Activa les opcions de localització"

#: antix-system.sh:75
msgid "Names"
msgstr "Noms"

#: antix-system.sh:77
msgid "Enable name change"
msgstr "Activa el canvi de nom"

#: antix-system.sh:91
msgid "Computer name"
msgstr "Nom de l'ordinador"

#: antix-system.sh:99
msgid "Computer domain"
msgstr "Domini de l'ordinador"

#: antix-system.sh:107
msgid "Samba work group"
msgstr "Grup de treball de SAMBA"

#: antix-system.sh:117
msgid "Locales"
msgstr "Locales"

#: antix-system.sh:135
msgid "Change language"
msgstr "Canvia el idioma"

#: antix-system.sh:139
msgid "enable language select"
msgstr "Activa la selecció de llenguatge"

#: antix-system.sh:145
msgid "select console locale"
msgstr "Tria el locale de la consola"

#: antix-system.sh:151
msgid "select keyboard map"
msgstr "Tria el mapa de teclat"

#: antix-system.sh:156
msgid "Change mouse order"
msgstr "Canvia l'ordre del ratolí"

#: antix-system.sh:161
msgid "Leave default"
msgstr "Deixa'l per omissió"

#: antix-system.sh:162
msgid "Left hand"
msgstr "Esquerrà"

#: antix-system.sh:163
msgid "Right hand"
msgstr "Dretà"

#: antix-system.sh:172
msgid "Disk management"
msgstr "Gestió de disc"

#: antix-system.sh:174
msgid "Enable Disk Management"
msgstr "Activa la gestió de disc"

#: antix-system.sh:188 antix-system.sh:287
msgid "Options"
msgstr "Opcions"

#: antix-system.sh:190
msgid "Clear Package Cache"
msgstr "Buida la memòria cau de paquets"

#: antix-system.sh:196
msgid "Clear Logs"
msgstr "Buida el registre"

#: antix-system.sh:202
msgid "Show basic disk usage"
msgstr "Mostra l'ús bàsic del disc"

#: antix-system.sh:208
msgid "Run partition manager"
msgstr "Executa el gestor de particions"

#: antix-system.sh:214
msgid "Run antiX to USB"
msgstr "Executa antiX a USB"

#: antix-system.sh:224
msgid "Repair grub"
msgstr "Repara el GRUB"

#: antix-system.sh:226
msgid "Enable grub repair"
msgstr "Activa la reparació del GRUB"

#: antix-system.sh:240
msgid "Where to install"
msgstr "On instal·lar"

#: antix-system.sh:242
msgid "Install on MBR"
msgstr "Instal·la al MBR"

#: antix-system.sh:248
msgid "Install on root partition"
msgstr "Instal·la a la partició arrel"

#: antix-system.sh:256
msgid "Select system boot disk"
msgstr "Trieu el disc d'arrencada del Sistema"

#: antix-system.sh:261 antix-system.sh:274
msgid "None selected"
msgstr "Cap seleccionat/da"

#: antix-system.sh:269
msgid "Select root partition"
msgstr "Trieu la partició arrel"

#: antix-system.sh:289
msgid "Update grub menu"
msgstr "Actualitza el menú de GRUB"

#: antix-system.sh:345 antix-system.sh:379
msgid "Choose your locale for antiX applications"
msgstr "Trieu el vostre locale per a les aplicacions d'antiX"

#: antix-system.sh:347
msgid "Locale chosen, antix application locale left default"
msgstr "Triat el locale, es deixa el locale d'aplicacions d'antiX per omissió"

#: antix-system.sh:373
#, sh-format
msgid "Language has changed to $LOCALE"
msgstr "S'ha canviat la llengua a $LOCALE"

#: antix-system.sh:381
msgid "Keymap left default"
msgstr "Es deixa el mapa de teclat per omissió"

#: antix-system.sh:397
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT)"
msgstr "Teclat configurat a $KEYMAP, torneu a arrencar i useu el mapa de teclat amb ALT+MAJ+(DRETA/ESQ.)"

#: antix-system.sh:401
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT). Mouse layout set to $MOUSE"
msgstr "Teclat configurat a $KEYMAP, torneu a arrencar i useu el mapa de teclat amb ALT+MAJ+(DRETA/ESQ). Ratolí configurat a $MOUSE"

#: antix-system.sh:425 antix-system.sh:444
msgid "Need to add some valid information to change names"
msgstr "Cal afegir alguna informació correcta per a canviar els noms"

#: antix-system.sh:429
#, sh-format
msgid "Samba workgroup has been changed from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Canviat el grup de treball de SAMBA de $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:434
#, sh-format
msgid "Domain name has been changed from $OLDDOMAIN to $DOMAIN"
msgstr "Canviat el nom de domini de $OLDDOMAIN a $DOMAIN"

#: antix-system.sh:437
#, sh-format
msgid ""
"Domain name has been changed from $OLDDOMAIN to $DOMAIN and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Canviat el nom de domini de $OLDDOMAIN a $DOMAIN i el grup de treball de SAMBA de $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:448
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Canviat el nom de l'ordinador de $OLDNAME a $NAME i el grup de treball de SAMBA de $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:453
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Domain name from "
"$OLDDOMAIN to $DOMAIN"
msgstr "Canviat el nom de l'ordinador de $OLDNAME a $NAME i el nom de domini de $OLDDOMAIN a $DOMAIN"

#: antix-system.sh:456
#, sh-format
msgid ""
"Computer name name has been changed from $OLDNAME to $NAME, Domain name from"
" $OLDDOMAIN to $DOMAIN and Samba workgroup from $OLDWORKGROUP to "
"$NEWWORKGROUP"
msgstr "Canviat el nom de l'ordinador de $OLDNAME a $NAME, el nom de domini de $OLDDOMAIN a $DOMAIN  i el grup de treball de SAMBA de $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:466
msgid "Need to add some valid information to change locale options"
msgstr "Cal afegi alguna informació correcta per canviar les opcions de locale"

#: antix-system.sh:470 antix-system.sh:482 antix-system.sh:496
#: antix-system.sh:508
#, sh-format
msgid "Mouse Layout set to $MOUSE"
msgstr "Disposició del ratolí establerta a $MOUSE"

#: antix-system.sh:523
msgid "Impossible error, radio buttons require at least one choice"
msgstr "Error impossible, els botons d'opció necessiten una tria, com a mínim"
