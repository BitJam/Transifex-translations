msgid "Samba work group"
msgstr "Grupo de trabalho Samba"

#: antix-system.sh:117
msgid "Locales"
msgstr "Regionalização"

#: antix-system.sh:135
msgid "Change language"
msgstr "Modificar idioma"

#: antix-system.sh:139
msgid "enable language select"
msgstr "Seleção de idioma"

#: antix-system.sh:145
msgid "select console locale"
msgstr "Regionalização do console"

#: antix-system.sh:151
msgid "select keyboard map"
msgstr "Mapa do teclado"

#: antix-system.sh:156
msgid "Change mouse order"
msgstr "Modificar configuração do mouse"

#: antix-system.sh:161
msgid "Leave default"
msgstr "Padrão"

#: antix-system.sh:162
msgid "Left hand"
msgstr "Canhoto"

#: antix-system.sh:163
msgid "Right hand"
msgstr "Destro"

#: antix-system.sh:172
msgid "Disk management"
msgstr "Gerenciamento do disco"

#: antix-system.sh:174
msgid "Enable Disk Management"
msgstr "Habilitar gerenciamento do disco"

#: antix-system.sh:188 antix-system.sh:287
msgid "Options"
msgstr "Opções"

#: antix-system.sh:190
msgid "Clear Package Cache"
msgstr "Limpar cache de pacotes"

#: antix-system.sh:196
msgid "Clear Logs"
msgstr "Limpar logs"

#: antix-system.sh:202
msgid "Show basic disk usage"
msgstr "Exibir uso básico do disco"

#: antix-system.sh:208
msgid "Run partition manager"
msgstr "Executar gerenciador de partições"

#: antix-system.sh:214
msgid "Run antiX to USB"
msgstr "Instalar antiX no pendrive"

#: antix-system.sh:224
msgid "Repair grub"
msgstr "Reparo do GRUB"

#: antix-system.sh:226
msgid "Enable grub repair"
msgstr "Habilitar reparo do GRUB"

#: antix-system.sh:240
msgid "Where to install"
msgstr "Local de instalação"

#: antix-system.sh:242
msgid "Install on MBR"
msgstr "Instalar no MBR"

#: antix-system.sh:248
msgid "Install on root partition"
msgstr "Instalar na partição raiz"

#: antix-system.sh:256
msgid "Select system boot disk"
msgstr "Selecionar disco de boot"

#: antix-system.sh:261 antix-system.sh:274
msgid "None selected"
msgstr "Nenhum selecionado"

#: antix-system.sh:269
msgid "Select root partition"
msgstr "Selecionar partição raiz"

#: antix-system.sh:289
msgid "Update grub menu"
msgstr "Atualizar menu do GRUB"

#: antix-system.sh:345 antix-system.sh:379
msgid "Choose your locale for antiX applications"
msgstr "Escolha a regionalização para os aplicativos do antiX"

#: antix-system.sh:347
msgid "Locale chosen, antix application locale left default"
msgstr "Regionalização escolhida, aplicativos do antiX deixados no padrão"

#: antix-system.sh:373
#, sh-format
msgid "Language has changed to $LOCALE"
msgstr "Idioma alterado para $LOCALE"

#: antix-system.sh:381
msgid "Keymap left default"
msgstr "Mapa do teclado deixado no padrão"

#: antix-system.sh:397
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT)"
msgstr "Teclado definido para $KEYMAP, reinicie e use o mapa com ALT + SHIFT + (ESQUERDA/DIREITA)"

#: antix-system.sh:401
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT). Mouse layout set to $MOUSE"
msgstr "Teclado definido para $KEYMAP, reinicie e use o mapa com ALT + SHIFT + (ESQUERDA/DIREITA). Disposição do mouse definida para $MOUSE"

#: antix-system.sh:425 antix-system.sh:444
msgid "Need to add some valid information to change names"
msgstr "É preciso incluir uma informação válida para modificar os nomes"

#: antix-system.sh:429
#, sh-format
msgid "Samba workgroup has been changed from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Grupo de trabalho Samba foi modificado de $OLDWORKGROUP para $NEWWORKGROUP"

#: antix-system.sh:434
#, sh-format
msgid "Domain name has been changed from $OLDDOMAIN to $DOMAIN"
msgstr "Nome de domínio foi modificado de $OLDDOMAIN para $DOMAIN"

#: antix-system.sh:437
#, sh-format
msgid ""
"Domain name has been changed from $OLDDOMAIN to $DOMAIN and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Nome de domínio foi modificado de $OLDDOMAIN para $DOMAIN e grupo de trabalho Samba de $OLDWORKGROUP para $NEWWORKGROUP"

#: antix-system.sh:448
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Nome da máquina foi modificado de $OLDNAME para $NAME e grupo de trabalho Samba de $OLDWORKGROUP para $NEWWORKGROUP"

#: antix-system.sh:453
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Domain name from "
"$OLDDOMAIN to $DOMAIN"
msgstr "Nome da máquina foi modificado de $OLDNAME para $NAME e nome de domínio foi modificado de $OLDDOMAIN para $DOMAIN"

#: antix-system.sh:456
#, sh-format
msgid ""
"Computer name name has been changed from $OLDNAME to $NAME, Domain name from"
" $OLDDOMAIN to $DOMAIN and Samba workgroup from $OLDWORKGROUP to "
"$NEWWORKGROUP"
msgstr "Nome da máquina foi modificado de $OLDNAME para $NAME, nome de domínio foi modificado de $OLDDOMAIN para $DOMAIN e grupo de trabalho Samba de $OLDWORKGROUP para $NEWWORKGROUP"

#: antix-system.sh:466
msgid "Need to add some valid information to change locale options"
msgstr "É preciso incluir uma informação válida para modificar as opções de regionalização"

#: antix-system.sh:470 antix-system.sh:482 antix-system.sh:496
#: antix-system.sh:508
#, sh-format
msgid "Mouse Layout set to $MOUSE"
msgstr "Disposição do mouse definida para $MOUSE"

#: antix-system.sh:523
msgid "Impossible error, radio buttons require at least one choice"
msgstr "Erro, os botões requerem ao menos uma escolha"
