#: antix-system.sh:345 antix-system.sh:379
msgid "Choose your locale for antiX applications"
msgstr "Kies uw taal voor antiX-applicaties"

#: antix-system.sh:347
msgid "Locale chosen, antix application locale left default"
msgstr "Taal gekozen. Taal voor antiX-applicaties blijft standaard"

#: antix-system.sh:373
#, sh-format
msgid "Language has changed to $LOCALE"
msgstr "Taal is ingesteld op $LOCALE"

#: antix-system.sh:381
msgid "Keymap left default"
msgstr "Toetsenbord-standaard behouden"

#: antix-system.sh:397
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT)"
msgstr "Toestenbord ingesteld op $KEYMAP, herstarten en indeling gebruiken met ALS+SHIFT+(LINKS/RECHTS)"

#: antix-system.sh:401
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT). Mouse layout set to $MOUSE"
msgstr "Toetsenbord ingesteld op $KEYMAP, herstarten en indeling gebruiken met ALT+SHIFT+(LINKS/RECHTS). Muis ingesteld op $MOUSE"

#: antix-system.sh:425 antix-system.sh:444
msgid "Need to add some valid information to change names"
msgstr "Er is enige bruikbare informatie nodig om de naam te veranderen"

#: antix-system.sh:429
#, sh-format
msgid "Samba workgroup has been changed from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Samba Werkgroep is veranderd van $OLDWORKGROUP naar $NEWWORKGROUP"

#: antix-system.sh:434
#, sh-format
msgid "Domain name has been changed from $OLDDOMAIN to $DOMAIN"
msgstr "Domein-naam is veranderd van $OLDDOMAIN naar $DOMAIN"

#: antix-system.sh:437
#, sh-format
msgid ""
"Domain name has been changed from $OLDDOMAIN to $DOMAIN and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Domein-naam is omgezet van $OLDDOMAIN naar $DOMAIN en de Samba Werkgroep van $OLDWORKGROUP naar $NEWWORKGROUP"

#: antix-system.sh:448
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "De computer-naam is omgezet van $OLDNAME naar $NAME en de Samba Werkgroep van $OLDWORKGROUP naar $NEWWORKGROUP"

#: antix-system.sh:453
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Domain name from "
"$OLDDOMAIN to $DOMAIN"
msgstr "De computer-naam is omgezet van $OLDNAME naar $NAME en de domeinnaam van $OLDDOMAIN naar $DOMAIN"

#: antix-system.sh:456
#, sh-format
msgid ""
"Computer name name has been changed from $OLDNAME to $NAME, Domain name from"
" $OLDDOMAIN to $DOMAIN and Samba workgroup from $OLDWORKGROUP to "
"$NEWWORKGROUP"
msgstr "De computer-naam is omgezet van $OLDNAME naar $NAME, de domeinnaam van $OLDDOMAIN naar $DOMAIN en de Samba Werkgroep van $OLDWORKGROUP naar $NEWWORKGROUP"

#: antix-system.sh:466
msgid "Need to add some valid information to change locale options"
msgstr "Er is enige bruikbare informatie nodig om de taalinstellingen te veranderen"

#: antix-system.sh:470 antix-system.sh:482 antix-system.sh:496
#: antix-system.sh:508
#, sh-format
msgid "Mouse Layout set to $MOUSE"
msgstr "Muis ingesteld op $MOUSE"

