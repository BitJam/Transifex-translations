msgid "Grub"
msgstr "Grub"

#: antix-system.sh:63
msgid "Locale options"
msgstr "Možnosti jazyka"

#: antix-system.sh:65 antix-system.sh:119
msgid "Enable locale options"
msgstr "Povolit možnosti jazyka"

#: antix-system.sh:75
msgid "Names"
msgstr "Názvy"

#: antix-system.sh:77
msgid "Enable name change"
msgstr "Povolit změnu názvu"

#: antix-system.sh:91
msgid "Computer name"
msgstr "Název počítače"

#: antix-system.sh:99
msgid "Computer domain"
msgstr "Doména počítače"

#: antix-system.sh:107
msgid "Samba work group"
msgstr "Pracovní skupina Samba"

#: antix-system.sh:117
msgid "Locales"
msgstr "Jazyky"

#: antix-system.sh:135
msgid "Change language"
msgstr "Změnit jazyk"

#: antix-system.sh:139
msgid "enable language select"
msgstr "povolit výběr jazyka"

#: antix-system.sh:145
msgid "select console locale"
msgstr "vybrat jazyk konzole"

#: antix-system.sh:151
msgid "select keyboard map"
msgstr "vybrat mapu klávesnice"

#: antix-system.sh:156
msgid "Change mouse order"
msgstr "Změnit pořadí myši"

#: antix-system.sh:161
msgid "Leave default"
msgstr "Nechat standardní"

#: antix-system.sh:162
msgid "Left hand"
msgstr "Pro leváky"

#: antix-system.sh:163
msgid "Right hand"
msgstr "Pro praváky"

#: antix-system.sh:172
msgid "Disk management"
msgstr "Správa disku"

#: antix-system.sh:174
msgid "Enable Disk Management"
msgstr "Povolit správu disku"

#: antix-system.sh:188 antix-system.sh:287
msgid "Options"
msgstr "Možnosti"

#: antix-system.sh:190
msgid "Clear Package Cache"
msgstr "Vyčistit mezipamět balíčku"

#: antix-system.sh:196
msgid "Clear Logs"
msgstr "Vyčistit záznamy"

#: antix-system.sh:202
msgid "Show basic disk usage"
msgstr "Zobrazit základní použití disku"

#: antix-system.sh:208
msgid "Run partition manager"
msgstr "Spustit správce oddílů"

#: antix-system.sh:214
msgid "Run antiX to USB"
msgstr "Spustit antiX na USB"

#: antix-system.sh:224
msgid "Repair grub"
msgstr "Opravit grub"

#: antix-system.sh:226
msgid "Enable grub repair"
msgstr "Povolit opravu grub"

#: antix-system.sh:240
msgid "Where to install"
msgstr "Kam nainstovat"

#: antix-system.sh:242
msgid "Install on MBR"
msgstr "Nainstalovat na MBR"

#: antix-system.sh:248
msgid "Install on root partition"
msgstr "Nainstalovat na kořenový oddíl"

#: antix-system.sh:256
msgid "Select system boot disk"
msgstr "Vybrat systémový disk pro zavádění"

#: antix-system.sh:261 antix-system.sh:274
msgid "None selected"
msgstr "Nic nevybráno"

#: antix-system.sh:269
msgid "Select root partition"
msgstr "Vybrat kořenový oddíl"

#: antix-system.sh:289
msgid "Update grub menu"
msgstr "Aktualizovat menu grub"

#: antix-system.sh:345 antix-system.sh:379
msgid "Choose your locale for antiX applications"
msgstr "Zvolit jazyk pro aplikace antiX"

#: antix-system.sh:347
msgid "Locale chosen, antix application locale left default"
msgstr "Jazyk zvolen, jazyk aplikací antix ponechán na výchozím"

#: antix-system.sh:373
#, sh-format
msgid "Language has changed to $LOCALE"
msgstr "Jazyk byl změněn na $LOCALE"

#: antix-system.sh:381
msgid "Keymap left default"
msgstr "Mapa klávesnice ponechána na výchozí"

#: antix-system.sh:397
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT)"
msgstr "Klávesnice nastavena na $KEYMAP, restartujte a mapu klávesnice požijte pomocí ALT + SHIFT + (DOLEVA/ DOPRAVA)"

#: antix-system.sh:401
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT). Mouse layout set to $MOUSE"
msgstr "Klávesnice nastavena na $KEYMAP, restartujte a mapu klávesnice požijte pomocí ALT + SHIFT + (DOLEVA/ DOPRAVA). Rozvržení myši nastaveno na $MOUSE"

#: antix-system.sh:425 antix-system.sh:444
msgid "Need to add some valid information to change names"
msgstr "Je třeba přidat jisté platné informace pro změnu názvu"

#: antix-system.sh:429
#, sh-format
msgid "Samba workgroup has been changed from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Pracovní skupina Samba změněna z $OLDWORKGROUP na $NEWWORKGROUP"

#: antix-system.sh:434
#, sh-format
msgid "Domain name has been changed from $OLDDOMAIN to $DOMAIN"
msgstr "Název domény změněn z $OLDDOMAIN na $DOMAIN"

#: antix-system.sh:437
#, sh-format
msgid ""
"Domain name has been changed from $OLDDOMAIN to $DOMAIN and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Název domény změněn z $OLDDOMAIN na $DOMAIN a pracovní skupina Samba změněna z $OLDWORKGROUP na $NEWWORKGROUP"

#: antix-system.sh:448
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Název počítače změněn z $OLDNAME na  $NAME a pracovní skupina Samba změněna z $OLDWORKGROUP na $NEWWORKGROUP"

#: antix-system.sh:453
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Domain name from "
"$OLDDOMAIN to $DOMAIN"
msgstr "Název počítače změněn z $OLDNAME na  $NAME a název domény změněn z $OLDDOMAIN na $DOMAIN"

#: antix-system.sh:456
#, sh-format
msgid ""
"Computer name name has been changed from $OLDNAME to $NAME, Domain name from"
" $OLDDOMAIN to $DOMAIN and Samba workgroup from $OLDWORKGROUP to "
"$NEWWORKGROUP"
msgstr "Název počítače změněn z $OLDNAME na  $NAME, Název domény změněn z $OLDDOMAIN na $DOMAIN a pracovní skupina Samba změněna z $OLDWORKGROUP na $NEWWORKGROUP"

#: antix-system.sh:466
msgid "Need to add some valid information to change locale options"
msgstr "Je třeba přidat jisté platné informace pro změnu jazyka"

#: antix-system.sh:470 antix-system.sh:482 antix-system.sh:496
#: antix-system.sh:508
#, sh-format
msgid "Mouse Layout set to $MOUSE"
msgstr "Rozvržení myši změněno na $MOUSE"

#: antix-system.sh:523
msgid "Impossible error, radio buttons require at least one choice"
msgstr "Nemožná chyba, přepínací tlačítka potřebují alespoň jednu volbu"
