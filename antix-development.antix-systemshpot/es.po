#: antix-system.sh:52
msgid "Locale"
msgstr "Localización"

#: antix-system.sh:53
msgid "Disk_Management"
msgstr "Manejo del disco"

#: antix-system.sh:54
msgid "Grub"
msgstr "Grub"

#: antix-system.sh:63
msgid "Locale options"
msgstr "Opciones de localización"

#: antix-system.sh:65 antix-system.sh:119
msgid "Enable locale options"
msgstr "Habilitar opciones de localizaciones"

#: antix-system.sh:75
msgid "Names"
msgstr "Nombres"

#: antix-system.sh:77
msgid "Enable name change"
msgstr "Habilitar el cambio de nombre"

#: antix-system.sh:91
msgid "Computer name"
msgstr "Nombre del sistema"

#: antix-system.sh:99
msgid "Computer domain"
msgstr "Dominio del sistema"

#: antix-system.sh:107
msgid "Samba work group"
msgstr "Grupo de trabajo Samba"

#: antix-system.sh:117
msgid "Locales"
msgstr "Localizaciones"

#: antix-system.sh:135
msgid "Change language"
msgstr "Cambiar el idioma"

#: antix-system.sh:139
msgid "enable language select"
msgstr "habilitar la selección del idioma"

#: antix-system.sh:145
msgid "select console locale"
msgstr "seleccionar localización para la consola"

#: antix-system.sh:151
msgid "select keyboard map"
msgstr "seleccionar una mapa de teclado"

#: antix-system.sh:156
msgid "Change mouse order"
msgstr "Cambiar el orden del ratón"

#: antix-system.sh:161
msgid "Leave default"
msgstr "Mantener las opciones por defecto"

#: antix-system.sh:162
msgid "Left hand"
msgstr "Mano izquierda"

#: antix-system.sh:163
msgid "Right hand"
msgstr "Mano derecha"

#: antix-system.sh:172
msgid "Disk management"
msgstr "Manejo del disco"

#: antix-system.sh:174
msgid "Enable Disk Management"
msgstr "Habilitar el manejo del disco"

#: antix-system.sh:188 antix-system.sh:287
msgid "Options"
msgstr "Opciones"

#: antix-system.sh:190
msgid "Clear Package Cache"
msgstr "Borrar el Cache de Paquetes"

#: antix-system.sh:196
msgid "Clear Logs"
msgstr "Borrar los historiales"

#: antix-system.sh:202
msgid "Show basic disk usage"
msgstr "Mostrar el uso básico del disco"

#: antix-system.sh:208
msgid "Run partition manager"
msgstr "Ejecutar un manejador de particiones"

#: antix-system.sh:214
msgid "Run antiX to USB"
msgstr "Ejecutar antix to USB"

#: antix-system.sh:224
msgid "Repair grub"
msgstr "Reparar Grub"

#: antix-system.sh:226
msgid "Enable grub repair"
msgstr "Habilitar la reparación de Grub"

#: antix-system.sh:240
msgid "Where to install"
msgstr "¿Dónde instalar?"

#: antix-system.sh:242
msgid "Install on MBR"
msgstr "Instalar en MBR"

#: antix-system.sh:248
msgid "Install on root partition"
msgstr "Instalar en la partición root"

#: antix-system.sh:256
msgid "Select system boot disk"
msgstr "Seleccionar el disco de inicio del sistema"

#: antix-system.sh:261 antix-system.sh:274
msgid "None selected"
msgstr "No se seleccionó ninguno"

#: antix-system.sh:269
msgid "Select root partition"
msgstr "Seleccionar la partición de root"

#: antix-system.sh:289
msgid "Update grub menu"
msgstr "Actualizar el menú de Grub"

#: antix-system.sh:345 antix-system.sh:379
msgid "Choose your locale for antiX applications"
msgstr "Escoger su localización para las aplicaciones de antiX"

#: antix-system.sh:347
msgid "Locale chosen, antix application locale left default"
msgstr "Escogió localización, aplicación de antiX se mantenga localización por defecto"

#: antix-system.sh:373
#, sh-format
msgid "Language has changed to $LOCALE"
msgstr "Se ha cambiado el idioma a $LOCALE"

#: antix-system.sh:381
msgid "Keymap left default"
msgstr "La mapa del teclado es la dada por defecto"

#: antix-system.sh:397
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT)"
msgstr "Se ajustó el teclado a $KEYMAP, reinicie y use la distribución nueva oprimiendo ALT + MAYÚSCULAS + (IZQUIERDA / DERECHA)"

#: antix-system.sh:401
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT). Mouse layout set to $MOUSE"
msgstr "Se ajustó el teclado a $KEYMAP, reinicie y use la distribución nueva oprimiendo ALT + MAYÚSCULAS + (IZQUIERDA / DERECHA). La distribución del ratón se ajustó a $MOUSE"

#: antix-system.sh:425 antix-system.sh:444
msgid "Need to add some valid information to change names"
msgstr "Debe agregar información válida para cambiar los nombres"

#: antix-system.sh:429
#, sh-format
msgid "Samba workgroup has been changed from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Se ha cambiado el grupo de trabajo Samba de $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:434
#, sh-format
msgid "Domain name has been changed from $OLDDOMAIN to $DOMAIN"
msgstr "El nombre del dominio se ha cambiado de $OLDDOMAIN a $DOMAIN"

#: antix-system.sh:437
#, sh-format
msgid ""
"Domain name has been changed from $OLDDOMAIN to $DOMAIN and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Se ha cambiado el nombre del dominio de $OLDDOMAIN a $DOMAIN y el grupo de trabajo Samba de $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:448
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "Se ha cambiado el nombre del sistema de $OLDNAME a $NAME y el grupo de trabajo Samba de $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:453
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Domain name from "
"$OLDDOMAIN to $DOMAIN"
msgstr "Se ha cambiado el nombre del sistema de $OLDNAME a $NAME y el nombre del dominio de $OLDDOMAIN a $DOMAIN"

#: antix-system.sh:456
#, sh-format
msgid ""
"Computer name name has been changed from $OLDNAME to $NAME, Domain name from"
" $OLDDOMAIN to $DOMAIN and Samba workgroup from $OLDWORKGROUP to "
"$NEWWORKGROUP"
msgstr "Se ha cambiado el nombre del sistema de $OLDNAME a $NAME, el dominio de $OLDDOMAIN a $DOMAIN y el grupo de trabajo Samba de $OLDWORKGROUP a $NEWWORKGROUP"

#: antix-system.sh:466
msgid "Need to add some valid information to change locale options"
msgstr "Debe agregar información válida para cambiar las opciones de localizaciones."

#: antix-system.sh:470 antix-system.sh:482 antix-system.sh:496
#: antix-system.sh:508
#, sh-format
msgid "Mouse Layout set to $MOUSE"
msgstr "Se cambió la distribución del ratón a $MOUSE"

#: antix-system.sh:523
msgid "Impossible error, radio buttons require at least one choice"
msgstr "Error imposible.  El radio de los botones requiere al menos una escogencia."
