#: antix-system.sh:373
#, sh-format
msgid "Language has changed to $LOCALE"
msgstr "تم تغيير اللغة إلى $LOCALE"

#: antix-system.sh:381
msgid "Keymap left default"
msgstr "خريطة المفاتيح تركت  على الإعداد الافتراضي"

#: antix-system.sh:397
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT)"
msgstr "تعيين لوحة المفاتيح إلى  $KEYMAP, الرجار إعادة التشغيل و إستعمال حريطة المفاتيح بالضغط على (ALT + SHIFT + (LEFT / RIGHT"

#: antix-system.sh:401
#, sh-format
msgid ""
"Keyboard set to $KEYMAP, restart and use keymap by ALT + SHIFT + (LEFT / "
"RIGHT). Mouse layout set to $MOUSE"
msgstr "تعيين لوحة المفاتيح إلى  $KEYMAP, الرجار إعادة التشغيل و إستعمال حريطة المفاتيح بالضغط على (ALT + SHIFT + (LEFT / RIGHT. تعيين تحطيط الماوس إلى $MOUSE"

#: antix-system.sh:425 antix-system.sh:444
msgid "Need to add some valid information to change names"
msgstr "نحتاج إلى إضافة بعض المعلومات الصحيحة لتغيير أسماء"

#: antix-system.sh:429
#, sh-format
msgid "Samba workgroup has been changed from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr " تغير اسم مجموعة عمل Samba من $OLDWORKGROUP إلى $NEWWORKGROUP"

#: antix-system.sh:434
#, sh-format
msgid "Domain name has been changed from $OLDDOMAIN to $DOMAIN"
msgstr "تغير اسم النطاق من $OLDDOMAIN إلى $DOMAIN"

#: antix-system.sh:437
#, sh-format
msgid ""
"Domain name has been changed from $OLDDOMAIN to $DOMAIN and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "تغير اسم النطاق من $OLDDOMAIN إلى $DOMAIN و  تغير اسم مجموعة عمل Samba من $OLDWORKGROUP إلى $NEWWORKGROUP"

#: antix-system.sh:448
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Samba workgroup "
"from $OLDWORKGROUP to $NEWWORKGROUP"
msgstr "تغير اسم الحاسوب من $OLDNAME إلى $NAME و  تغير اسم مجموعة عمل Samba من $OLDWORKGROUP إلى $NEWWORKGROUP"

#: antix-system.sh:453
#, sh-format
msgid ""
"Computer name has been changed from $OLDNAME to $NAME and Domain name from "
"$OLDDOMAIN to $DOMAIN"
msgstr "تغير اسم الحاسوب من $OLDNAME إلى $NAME و تغير اسم النطاق من $OLDDOMAIN إلى $DOMAIN"

#: antix-system.sh:456
#, sh-format
msgid ""
"Computer name name has been changed from $OLDNAME to $NAME, Domain name from"
" $OLDDOMAIN to $DOMAIN and Samba workgroup from $OLDWORKGROUP to "
"$NEWWORKGROUP"
msgstr "تغير اسم الحاسوب من $OLDNAME إلى $NAME و تغير اسم النطاق من $OLDDOMAIN إلى $DOMAIN و  تغير اسم مجموعة عمل Samba من $OLDWORKGROUP إلى $NEWWORKGROUP"

#: antix-system.sh:466
msgid "Need to add some valid information to change locale options"
msgstr "تحتاج إلى إضافة بعض المعلومات الصحيحة لتغيير خيارات اللغة"

#: antix-system.sh:470 antix-system.sh:482 antix-system.sh:496
