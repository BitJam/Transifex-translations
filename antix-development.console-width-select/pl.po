msgid "This program must be run as root"
msgstr "Ten program musi być uruchamiany jako administrator"

msgid "Could not get console width. Is the framebuffer enabled?"
msgstr "Nie można uzyskać szerokości konsoli. Czy framebuffer jest włączony?"

msgid "No fonts found"
msgstr "Nie znaleziono czcionek"

msgid "Select the console width in characters"
msgstr "Wybierz szerokość konsoli w znakach"

msgid "Press: <Enter> to select a font <q> to quit, <h> for help"
msgstr ""
"Naciśnij: <Enter> aby wybrać font (czcionkę), <q> aby zakończyć, <h> aby "
"uzyskać pomoc"

msgid "Screen is %s chars wide"
msgstr "Ekran ma szerokość %s znaków"

msgid "%s is %s chars wide"
msgstr "%s ma szerokość %s znaków"

msgid "Select Console Width"
msgstr "Wybierz szerokość konsoli"

msgid "Screen is %s chars wide, font is %s"
msgstr "Ekran jest szeroki na %s znaków, czcionka to %s"

#. How many columns wide the screen is, and the font width
msgid "columns"
msgstr "kolumny"

msgid "font width"
msgstr "szerokość czcionki"

msgid "Setting default font to %s"
msgstr "Ustawienie domyślnej czcionki na %s"
