#. Keyboard keys: up-arrow, down-arrow, page-up, page-down
msgid "up-arrow"
msgstr "upp-ör"

msgid "down-arrow"
msgstr "niður-ör"

msgid "page-up"
msgstr "síða-upp"

msgid "page-down"
msgstr "síða-niður"

#. Keyboard keys: left-arrow, right-arrow
msgid "left-arrow"
msgstr "vinstri-ör"

msgid "right-arrow"
msgstr "hægri-ör"

msgid "No controller found"
msgstr "Engin stýring fannst"

msgid "You need to be root to run this script"
msgstr "Þú verður að vera kerfisstjóri/root til að keyra þessa skriftu"

msgid "Backlight Brightness Control"
msgstr "Stýring á styrk baklýsingar"

msgid "Use <%s> and <%s> to adjust the brightness"
msgstr "Notaðu <%s> og <%s> til að breyta birtustiginu"

msgid "Use <%s> and <%s> for smaller steps"
msgstr "Notaðu <%s> og <%s> fyrir smærri þrep"

msgid "Use <%s> and <%s> for bigger steps"
msgstr "Notaðu <%s> og <%s> fyrir stærri þrep"

msgid "Press '%s' to quit"
msgstr "Ýttu á '%s' til að hætta"
