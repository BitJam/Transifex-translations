#. Keyboard keys: up-arrow, down-arrow, page-up, page-down
msgid "up-arrow"
msgstr "ቀስት ወደ-ላይ"

msgid "down-arrow"
msgstr "ቀስት ወደ-ታች"

msgid "page-up"
msgstr "ገጽ ወደ-ላይ"

msgid "page-down"
msgstr "ገጽ ወደ-ታች"

#. Keyboard keys: left-arrow, right-arrow
msgid "left-arrow"
msgstr "የ ግራ-ቀስት"

msgid "right-arrow"
msgstr "የ ቀኝ-ቀስት"

msgid "No controller found"
msgstr "ምንም መቆጣጠሪያ አልተገኘም"

msgid "You need to be root to run this script"
msgstr "ይህን ጽሁፍ ለ ማስኬድ እርስዎ root መሆን አለብዎት"

msgid "Backlight Brightness Control"
msgstr "የ መደብ ብርሃን መቆጣጠሪያ"

msgid "Use <%s> and <%s> to adjust the brightness"
msgstr "ይጠቀሙ <%s> እና <%s> ብሩህነት ለ ማስተካከል"

msgid "Use <%s> and <%s> for smaller steps"
msgstr "ይጠቀሙ <%s> እና <%s> ለ አነስተኛ ደረጃዎች"

msgid "Use <%s> and <%s> for bigger steps"
msgstr "ይጠቀሙ <%s> እና <%s> ለ ከፍተኛ ደረጃዎች"

msgid "Press '%s' to quit"
msgstr "ይጫኑ '%s' ለ ማጥፋት"
