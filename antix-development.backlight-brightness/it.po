#. Keyboard keys: up-arrow, down-arrow, page-up, page-down
msgid "up-arrow"
msgstr "freccia su"

msgid "down-arrow"
msgstr "freccia giù"

msgid "page-up"
msgstr "pagina su"

msgid "page-down"
msgstr "pagina giù"

#. Keyboard keys: left-arrow, right-arrow
msgid "left-arrow"
msgstr "freccia sinistra"

msgid "right-arrow"
msgstr "freccia destra"

msgid "No controller found"
msgstr "Nessun controller trovato"

msgid "You need to be root to run this script"
msgstr "Devi essere un utente root per eseguire questo script"

msgid "Backlight Brightness Control"
msgstr "Controllo luminosità della retroilluminazione"

msgid "Use <%s> and <%s> to adjust the brightness"
msgstr "Usa <%s> e <%s> per regolare la luminosità"

msgid "Use <%s> and <%s> for smaller steps"
msgstr "Usa <%s> e <%s> per una regolazione graduale"

msgid "Use <%s> and <%s> for bigger steps"
msgstr "Usa <%s> e <%s> per valori di regolazione più grandi"

msgid "Press '%s' to quit"
msgstr "Premi '%s' per uscire"
