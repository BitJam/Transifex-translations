#. Keyboard keys: up-arrow, down-arrow, page-up, page-down
msgid "up-arrow"
msgstr "op-pil"

msgid "down-arrow"
msgstr "ned-pil"

msgid "page-up"
msgstr "page-up"

msgid "page-down"
msgstr "page-down"

#. Keyboard keys: left-arrow, right-arrow
msgid "left-arrow"
msgstr "venstre-pil"

msgid "right-arrow"
msgstr "højre-pil"

msgid "No controller found"
msgstr "Ingen styring fundet"

msgid "You need to be root to run this script"
msgstr "Du skal være root for at kunne køre scriptet"

msgid "Backlight Brightness Control"
msgstr "Styring af baggrundsbelysningens lysstyrke"

msgid "Use <%s> and <%s> to adjust the brightness"
msgstr "Brug <%s> og <%s> til at justere lysstyrken"

msgid "Use <%s> and <%s> for smaller steps"
msgstr "Brug <%s> og <%s> til små trin"

msgid "Use <%s> and <%s> for bigger steps"
msgstr "Brug <%s> og <%s> til store trin"

msgid "Press '%s' to quit"
msgstr "Tryk på '%s' for at afslutte"
