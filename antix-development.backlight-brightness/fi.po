#. Keyboard keys: up-arrow, down-arrow, page-up, page-down
msgid "up-arrow"
msgstr "nuoli ylös"

msgid "down-arrow"
msgstr "nuoli-alas"

msgid "page-up"
msgstr "page-up"

msgid "page-down"
msgstr "page down"

#. Keyboard keys: left-arrow, right-arrow
msgid "left-arrow"
msgstr "vasen nuoli"

msgid "right-arrow"
msgstr "oikea nuoli"

msgid "No controller found"
msgstr "Ohjainta ei löytynyt"

msgid "You need to be root to run this script"
msgstr "Sinun täytyy olla pääkäyttäjä suorittaaksesi tämän skriptin"

msgid "Backlight Brightness Control"
msgstr "Taustavalaistuksen kirkkauden säätö"

msgid "Use <%s> and <%s> to adjust the brightness"
msgstr "Käytä <%s> ja <%s> kirkkauden säätämiseksi"

msgid "Use <%s> and <%s> for smaller steps"
msgstr "Käytä <%s> ja <%s> pienentääksesi"

msgid "Use <%s> and <%s> for bigger steps"
msgstr "Käytä <%s> ja <%s> suurentaaksesi"

msgid "Press '%s' to quit"
msgstr "Paina '%s' lopettaaksesi"
