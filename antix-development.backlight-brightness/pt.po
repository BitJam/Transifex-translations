#. Keyboard keys: up-arrow, down-arrow, page-up, page-down
msgid "up-arrow"
msgstr "seta para cima"

msgid "down-arrow"
msgstr "seta para baixo"

msgid "page-up"
msgstr "página para acima"

msgid "page-down"
msgstr "página para baixo"

#. Keyboard keys: left-arrow, right-arrow
msgid "left-arrow"
msgstr "seta para a esquerda"

msgid "right-arrow"
msgstr "seta para a direita"

msgid "No controller found"
msgstr "Não foram encontrados controladores"

msgid "You need to be root to run this script"
msgstr ""
"Precisa de ser administrador (root) para executar esta sequência de "
"instruções (script)."

msgid "Backlight Brightness Control"
msgstr "Controlo do brilho da retro-iluminação"

msgid "Use <%s> and <%s> to adjust the brightness"
msgstr "Use <%s> e <%s> para ajustar o brilho"

msgid "Use <%s> and <%s> for smaller steps"
msgstr "Use <%s> e <%s> para ajustes menores"

msgid "Use <%s> and <%s> for bigger steps"
msgstr "Use <%s> e <%s> para ajustes maiores"

msgid "Press '%s' to quit"
msgstr "Premir '%s' para sair"
