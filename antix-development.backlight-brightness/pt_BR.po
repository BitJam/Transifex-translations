#. Keyboard keys: up-arrow, down-arrow, page-up, page-down
msgid "up-arrow"
msgstr "seta para cima"

msgid "down-arrow"
msgstr "seta para baixo"

msgid "page-up"
msgstr "página pra cima"

msgid "page-down"
msgstr "página pra baixo"

#. Keyboard keys: left-arrow, right-arrow
msgid "left-arrow"
msgstr "seta para esquerda"

msgid "right-arrow"
msgstr "seta para direita"

msgid "No controller found"
msgstr "Nenhum controlador encontrado"

msgid "You need to be root to run this script"
msgstr "Você precisa ser raiz para rodar este roteiro"

msgid "Backlight Brightness Control"
msgstr "Controle de Brilho de Luz de Fundo"

msgid "Use <%s> and <%s> to adjust the brightness"
msgstr "Use <%s> e <%s> para ajustar o brilho"

msgid "Use <%s> and <%s> for smaller steps"
msgstr "Use <%s> e <%s> para passos menores"

msgid "Use <%s> and <%s> for bigger steps"
msgstr "Use <%s> e <%s> para passos maiores"

msgid "Press '%s' to quit"
msgstr "Aperte '%s' para sair"
