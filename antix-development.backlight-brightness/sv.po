#. Keyboard keys: up-arrow, down-arrow, page-up, page-down
msgid "up-arrow"
msgstr "upp-pil"

msgid "down-arrow"
msgstr "ner-pil"

msgid "page-up"
msgstr "sida-upp"

msgid "page-down"
msgstr "sida-ner"

#. Keyboard keys: left-arrow, right-arrow
msgid "left-arrow"
msgstr "vänster-pil"

msgid "right-arrow"
msgstr "höger-pil"

msgid "No controller found"
msgstr "Ingen controller hittad"

msgid "You need to be root to run this script"
msgstr "Du behöver vara root för att köra detta skript"

msgid "Backlight Brightness Control"
msgstr "Kontroll för Bakgrundsljusets styrka"

msgid "Use <%s> and <%s> to adjust the brightness"
msgstr "Använd  <%s> och <%s> för att justera ljusstyrkan"

msgid "Use <%s> and <%s> for smaller steps"
msgstr "Använd <%s> och <%s> för mindre steg"

msgid "Use <%s> and <%s> for bigger steps"
msgstr "Använd <%s> och <%s> för större steg"

msgid "Press '%s' to quit"
msgstr "Tryck på '%s' för att avsluta"
