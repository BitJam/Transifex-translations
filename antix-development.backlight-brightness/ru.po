#. Keyboard keys: up-arrow, down-arrow, page-up, page-down
msgid "up-arrow"
msgstr "Стрелка вверх"

msgid "down-arrow"
msgstr "Стрелка вниз"

msgid "page-up"
msgstr "Страница вверх"

msgid "page-down"
msgstr "Страница вниз"

#. Keyboard keys: left-arrow, right-arrow
msgid "left-arrow"
msgstr "Стрелка влево"

msgid "right-arrow"
msgstr "Стрелка вправо"

msgid "No controller found"
msgstr "Нет найденного контроллера"

