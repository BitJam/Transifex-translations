msgid "Use <%s> and <%s> for smaller steps"
msgstr "Используйте <%s> и <%s> для меньшего шага подстройки"

msgid "Use <%s> and <%s> for bigger steps"
msgstr "Используйте <%s> и <%s> для большего шага подстройки"

msgid "Press '%s' to quit"
msgstr "Нажмите '%s' для выхода"
