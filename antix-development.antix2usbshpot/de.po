#: antix2usb.sh:114
msgid "Error: ${exitval} - ${@}"
msgstr "Fehler: ${exitval} - ${@}"

#: antix2usb.sh:136
msgid "Info: $1"
msgstr "Info: $1"

#: antix2usb.sh:150
msgid "Continue? (y/n) "
msgstr "Fortfahren? (y/n)"

#: antix2usb.sh:154
msgid "Exiting"
msgstr "Verlassen"

#: antix2usb.sh:206
msgid "Command line: $PROGNAME $*"
msgstr "Befehlszeile: $PROGNAME $*"

#: antix2usb.sh:215
msgid "Error parsing command line args"
msgstr "Syntaxfehler in Befehlszeilen-Argument"

#: antix2usb.sh:295
msgid "==== Missing parameters: iso and/or device."
msgstr "==== Fehlende Parameter: ISO und/oder Gerät."

#: antix2usb.sh:314
#, sh-format
msgid "Cannot access iso file: \\'$ISO_FILENAME\\'"
msgstr "Kann nicht auf ISO-Datei zugreifen: \\'$ISO_FILENAME\\'"

#: antix2usb.sh:323
#, sh-format
msgid "Device $DEVICE is not a valid USB device"
msgstr "Gerät $DEVICE ist kein gültiges USB-Gerät"

#: antix2usb.sh:327
#, sh-format
msgid ""
"Invalid -p|--persist option \\'$PERSIST\\': should be \\'root\\' or "
"\\'home\\' or \\'both\\'"
msgstr "Ungültige -p|--persist Option \\'$PERSIST\\': sollte \\'root\\' oder \\'home\\' oder \\'beides\\' sein"

#: antix2usb.sh:340
msgid "Invalid --format option: should be ext2, ext3 or fat32"
msgstr "Ungültige --format Option: sollte ext2, ext3 oder FAT32 sein"

#: antix2usb.sh:349
#, sh-format
msgid "Cannot install syslinux bootloader on a $PARTITION_FS partition"
msgstr "Kann nicht syslinux Bootloader auf einer $PARTITION_FS Partition installieren"

#: antix2usb.sh:351
#, sh-format
msgid "Cannot install extlinux bootloader on a $PARTITION_FS partition"
msgstr "Kann nicht extlinux Bootloader auf einer $PARTITION_FS Partition installieren"

#: antix2usb.sh:381
msgid "Invalid label, allowed characters are: A-Z a-z 0-9 - _"
msgstr "Ungültiges Label, zulässige Zeichen sind: A-Z a-z 0-9 - _"

#: antix2usb.sh:385
msgid "Invalid label: length exceeds 16 characters"
msgstr "Ungültiges Label: die Länge darf 16 Zeichen nicht überschreiten"

#: antix2usb.sh:408 antix2usb.sh:484
msgid "No USB device found"
msgstr "Kein USB-Gerät gefunden"

#: antix2usb.sh:411
msgid "Device    Model                       Size"
msgstr "Geräte-Modell Größe"

#: antix2usb.sh:471
#, sh-format
msgid "Cannot access USB device $device. Sorry"
msgstr "Kann nicht auf USB-Gerät $device zugreifen. Entschuldigung"

#: antix2usb.sh:515
msgid "syslinux appears not installed. Should I install it?"
msgstr "Syslinux scheint nicht installiert zu sein. Soll ich es installieren?"

#: antix2usb.sh:520
msgid "Failed to install syslinux"
msgstr "Fehler beim installieren von Syslinux"

#: antix2usb.sh:529
msgid "extlinux appears not installed. Should I install it?"
msgstr "Extlinux scheint nicht installiert zu sein. Soll ich es installieren?"

#: antix2usb.sh:534
msgid "Failed to install extlinux"
msgstr "Fehler beim installieren von Extlinux"

#: antix2usb.sh:544
#, sh-format
msgid "Invalid boot mode: \\'$BOOT_MODE\\'. Exiting"
msgstr "Ungültiger Boot-Modus: \\'$BOOT_MODE\\'. Beendet"

#: antix2usb.sh:573
msgid "ISO file doesn't fit in partition"
msgstr "Die ISO-Datei passt nicht in die Partition"

#: antix2usb.sh:594
#, sh-format
msgid "Estimated write speed: $speed MB/s"
msgstr "Geschätzte Schreibgeschwindigkeit: §speed MB/s"

#: antix2usb.sh:637
#, sh-format
msgid "$vol appears to be mounted! Unmount it, and retry. Exiting."
msgstr "$vol scheint eingehängt zu sein! Aushängen, und versuchen Sie es erneut. Beendet."

#: antix2usb.sh:650
#, sh-format
msgid "Preparing bootable fat32 partition on device $device"
msgstr "Vorbereiten der bootfähigen FAT32-Partition auf dem Gerät $device"

#: antix2usb.sh:655
#, sh-format
msgid "Preparing bootable ext2 partition on device $device"
msgstr "Vorbereiten der bootfähigen ext2-Partition auf dem Gerät $device "

#: antix2usb.sh:661
#, sh-format
msgid "Preparing bootable ext3 partition on device $device"
msgstr "Vorbereiten der bootfähigen ext3-Partition auf dem Gerät $device"

#: antix2usb.sh:667
#, sh-format
msgid "Invalid file system type: \\'$fs\\'."
msgstr "Ungültiger Dateisystemtyp: \\'$fs\\'."

#: antix2usb.sh:673
#, sh-format
msgid "USB device: $device Model: $DEVICE_INFO Size: $DEVICE_SIZE MB"
msgstr "USB-Gerät: $device Modell: $DEVICE_INFO Größe: $DEVICE_SIZE MB"

#: antix2usb.sh:674
#, sh-format
msgid "Ready to format $device. This will erase all data on the device."
msgstr "Bereit zu formatieren $device. Dadurch werden alle Daten auf dem Gerät gelöscht."

#: antix2usb.sh:677
msgid "Clearing partition table"
msgstr "Clearing der Partitionstabelle"

#: antix2usb.sh:679
#, sh-format
msgid "Failed to zero $device"
msgstr "Fehler bei Null $device"

#: antix2usb.sh:686
#, sh-format
msgid "Creating bootable partition fs: ${fs}"
msgstr "Erstelle bootfähige Partition fs: ${fs}"

#: antix2usb.sh:687
msgid "Running fdisk.distrib..."
msgstr "Ausführung von fdisk.distrib..."

#: antix2usb.sh:726
#, sh-format
msgid "Failed to create partition $partition on device: $device"
msgstr "Fehler beim Erstellen der Partition $partition auf dem Gerät : $device"

#: antix2usb.sh:735
#, sh-format
msgid "Creating file system on $partition1, please wait..."
msgstr "Erstelle Dateisystem auf $partition1, bitte warten Sie..."

#: antix2usb.sh:738
#, sh-format
msgid "Failed to format $partition1"
msgstr "Formatierungsfehler $partition1"

#: antix2usb.sh:740
#, sh-format
msgid "Prepared bootable partition: $partition1 label: $label1"
msgstr "Vorbereiteten bootfähige Partition: $partition1 Label: $label1"

#: antix2usb.sh:756
#, sh-format
msgid "Failed to mount $partition1 on $usb_mountpoint"
msgstr "Fehler beim Einhängen der $partition1 auf den $usb_mountpoint"

#: antix2usb.sh:761
#, sh-format
msgid "Cannot create boot directory on $usb_mountpoint"
msgstr "Kann kein boot-Verzeichnis auf $usb_mountpoint erstellen"

#: antix2usb.sh:763
msgid "USB device mount point ready"
msgstr "USB-Gerät Einhängpunkt ist bereit"

#: antix2usb.sh:773
#, sh-format
msgid "Mounting $iso_file on temporary mount point"
msgstr "Einhängen der $iso_Datei auf temporären Einhängepunkt"

#: antix2usb.sh:776
#, sh-format
msgid "Failed to mount $iso_file on $iso_mountpoint"
msgstr "Fehler beim Einhängen der $iso_Datei auf den $iso_mountpoint"

#: antix2usb.sh:779
msgid "Iso file mount point ready"
msgstr "ISO-Datei Einhängpunkt ist bereit"

#: antix2usb.sh:790
#, sh-format
msgid "Unmounting $partition1 (please wait...)"
msgstr "Aushängen $partition1 (Bitte warten Sie...)"

#: antix2usb.sh:795
#, sh-format
msgid "Failed to remove USB mount point: $usb_mountpoint"
msgstr "Fehler beim Entfernen des USB Einhängpunkt: $usb_mountpoint"

#: antix2usb.sh:800
#, sh-format
msgid "Failed to remove ISO mount point: $iso_mountpoint"
msgstr "Fehler beim Entfernen des ISO Einhängpunkt: $iso_mountpoint"

#: antix2usb.sh:802
msgid "Mount points removed."
msgstr "Einhängepunkte entfernt."

#: antix2usb.sh:806
msgid "Process completed."
msgstr "Prozess abgeschlossen"

#: antix2usb.sh:809
msgid "Process failed"
msgstr "Prozess fehlgeschlagen"

#: antix2usb.sh:827 antix2usb.sh:909 antix2usb.sh:991
msgid "USB mount point undefined"
msgstr "USB-Einhängepunkt nicht definiert"

#: antix2usb.sh:835
#, sh-format
msgid "Failed to create directory ${usb_mountpoint}/antiX"
msgstr "Fehler beim Erstellen des Verzeichnisses ${usb_mountpoint}/antiX"

#: antix2usb.sh:841
#, sh-format
msgid ""
"Copying antiX compressed file system (${ISO_FILE_SIZE}) to the mounted "
"device"
msgstr "Kopieren des komprimierten antiX-Dateisystem (${ISO_FILE_SIZE}) auf das eingehängte Gerät"

#: antix2usb.sh:844
msgid "This will take a few minutes, relax..."
msgstr "Dies kann einige Minuten in Anspruch nehmen, entspannen Sie sich..."

#: antix2usb.sh:847
#, sh-format
msgid "Failed to cp ${iso_mountpoint}/antiX"
msgstr "Gescheitert cp ${iso_mountpoint}/antiX"

#: antix2usb.sh:850
msgid "Files copied to mounted device"
msgstr "Dateien zum eingehängten Gerät kopiert"

#: antix2usb.sh:852
msgid "Copying files to boot directory"
msgstr "Kopieren von Dateien in das boot-Verzeichnis"

#: antix2usb.sh:858
#, sh-format
msgid ""
"Failed to copy file ${iso_mountpoint}/boot/${i} to ${usb_mountpoint}/antiX"
msgstr "Fehler beim Kopieren der Datei ${iso_mountpoint}/boot/${i} zu ${usb_mountpoint}/antiX"

#: antix2usb.sh:866
#, sh-format
msgid "Failed to copy file ${iso_mountpoint}/${i} to ${usb_mountpoint}"
msgstr "Fehler beim Kopieren der Datei ${iso_mountpoint}/${i} zu ${usb_mountpoint}/antiX"

#: antix2usb.sh:874
#, sh-format
msgid ""
"Failed to copy file ${iso_mountpoint}/boot/${i} to ${usb_mountpoint}/boot"
msgstr "Fehler beim Kopieren der Datei ${iso_mountpoint}/boot/${i} zu ${usb_mountpoint}/boot"

#: antix2usb.sh:891 antix2usb.sh:1116
msgid "Invalid boot mode"
msgstr "Ungültiger Bootmodus"

#: antix2usb.sh:893
msgid "Copy completed"
msgstr "Kopieren abgeschlossen"

#: antix2usb.sh:915
msgid "Do you want to set up live persistence? [y|N]"
msgstr "Wollen Sie Live-Persistenz einrichten? [y|N]"

#: antix2usb.sh:921
#, sh-format
msgid "You can set up $PRF and/or $PHF "
msgstr "Sie können auch verschiedene $PRF und/oder $PHF"

#: antix2usb.sh:923
#, sh-format
msgid "Do you want to set up $PRF ? [y|N]"
msgstr "Wollen Sie $PRF einrichten? [y|N]"

#: antix2usb.sh:927
#, sh-format
msgid "Set up $PRF"
msgstr "Einrichten $PRF"

#: antix2usb.sh:928
msgid ""
"Type in file size. Make sure you have enough space. Just press Enter for "
"default 512 (MB) "
msgstr "Geben Sie die Dateigröße an. Achten Sie darauf, genügend Platz haben. Drücken Sie die Eingabetaste für Standard 512 (MB)"

#: antix2usb.sh:934 antix2usb.sh:955
msgid ""
"Type in file type: ext2, ext3 or ext4. Just press Enter for recommended "
"default ext2 "
msgstr "Geben Sie den Dateityp an: ext2, ext3 oder ext4. Drücken Sie die Eingabetaste für den empfohlenen Standard ext2"

#: antix2usb.sh:942
#, sh-format
msgid "$PRF not set up"
msgstr "$PRF nicht einrichten"

#: antix2usb.sh:944
#, sh-format
msgid "Do you want to set up $PHF ? [y|N]"
msgstr "Wollen Sie $PHF einrichten? [y|N]"

#: antix2usb.sh:948
#, sh-format
msgid "Set up $PHF"
msgstr "Einrichten $PHF"

#: antix2usb.sh:949
msgid "Type in file size. Just press Enter for default 128 (MB)"
msgstr "Geben Sie die Dateigröße an. Drücken Sie die Eingabetaste für Standard 128 (MB)"

#: antix2usb.sh:963
#, sh-format
msgid "$PHF not set up"
msgstr "$PHF nicht einrichten"

#: antix2usb.sh:966
msgid "Finished."
msgstr "Fertig."

#: antix2usb.sh:967
#, sh-format
msgid "File(s) have been saved to $usb_mountpoint/antiX "
msgstr "Datei(en) in $usb_mountpoint/antiX gespeichert"

#: antix2usb.sh:971
msgid "No persistence set up."
msgstr "Keine Persistenz einrichten."

#: antix2usb.sh:1019
#, sh-format
msgid "Preparing root persistence (size $CFS MB)..."
msgstr "Vorbereiten der root-Persistenz (size $CFS MB)..."

#: antix2usb.sh:1029
#, sh-format
msgid "Preparing home persistence (size $CFS MB)..."
msgstr "Vorbereiten der home-Persistenz (size $CFS MB)..."

#: antix2usb.sh:1049
msgid "Copying isolinux config files from iso"
msgstr "Kopiere isolinux Konfigurationsdateien von der ISO"

#: antix2usb.sh:1065
msgid ""
"Copying isolinux directory and config files from iso and configuring for "
"extlinux"
msgstr "Kopiere isolinux Verzeichnis und Konfigurationsdateien von der ISO und konfiguriere extlinux"

#: antix2usb.sh:1086
msgid ""
"Copying grub directory and config files from iso and configuring for grub"
msgstr "Kopiere Grub-Verzeichnis und Konfigurationsdateien von der ISO und konfiguriere Grub"

#: antix2usb.sh:1089
#, sh-format
msgid "Failed to copy grub configuaration in ${usb_mountpoint}/boot/grub"
msgstr "Fehler beim Kopieren der Grub-Konfiguration in ${usb_mountpoint}/boot/grub"

#: antix2usb.sh:1133
msgid "Installation of boot loader syslinux failed."
msgstr "Installation des Bootloader syslinux fehlgeschlagen."

#: antix2usb.sh:1137 antix2usb.sh:1160
msgid "Installation of master boot record failed."
msgstr "Installation im Master-Boot-Record fehlgeschlagen."

#: antix2usb.sh:1139
#, sh-format
msgid "syslinux successfully installed to $partition1"
msgstr "Syslinux erfolgreich installiert in $partition1"

#: antix2usb.sh:1155
msgid "Installation of boot loader extlinux failed."
msgstr "Installation des Bootloader extlinux fehlgeschlagen."

#: antix2usb.sh:1162
#, sh-format
msgid "extlinux successfully installed to $partition1"
msgstr "Extlinux erfolgreich installiert in $partition1"

#: antix2usb.sh:1199
#, sh-format
msgid "Installing grub on $device, please wait..."
msgstr "Installiere Grub in $device, Bitte warten Sie..."

#: antix2usb.sh:1204
#, sh-format
msgid "$GRUB_INSTALL failed, root-directory=$mountpoint, device=$device"
msgstr "$GRUB_INSTALL fehlgeschlagen, root-directory=$mountpoint, device=$device"

#: antix2usb.sh:1234
#, sh-format
msgid "Grub successfully installed to $device"
msgstr "Grub erfolgreich installiert auf $device"

#: antix2usb.sh:1301
msgid "You need to be root to run this script."
msgstr "Sie müssen root sein, um das Skript auszuführen."
