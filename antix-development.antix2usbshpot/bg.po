#: antix2usb.sh:114
msgid "Error: ${exitval} - ${@}"
msgstr "Грешка: ${exitval} - ${@}"

#: antix2usb.sh:136
msgid "Info: $1"
msgstr "Инфо: $1"

#: antix2usb.sh:150
msgid "Continue? (y/n) "
msgstr "Продължаване? (y/n) "

#: antix2usb.sh:154
msgid "Exiting"
msgstr "Излизане"

#: antix2usb.sh:206
msgid "Command line: $PROGNAME $*"
msgstr "Командна линия: $PROGNAME $*"

#: antix2usb.sh:215
msgid "Error parsing command line args"
msgstr "Грешка при предаването на командните аргументи"

#: antix2usb.sh:295
msgid "==== Missing parameters: iso and/or device."
msgstr "==== Липсващи параметри: iso и/или устройство."

#: antix2usb.sh:314
#, sh-format
msgid "Cannot access iso file: \\'$ISO_FILENAME\\'"
msgstr "Няма достъп до iso файл: \\'$ISO_FILENAME\\'"

#: antix2usb.sh:323
#, sh-format
msgid "Device $DEVICE is not a valid USB device"
msgstr "Устройство $DEVICE не е валидно USB устройство"

#: antix2usb.sh:327
#, sh-format
msgid ""
"Invalid -p|--persist option \\'$PERSIST\\': should be \\'root\\' or "
"\\'home\\' or \\'both\\'"
msgstr "Невалидна -p|--persist опция \\'$PERSIST\\': трябва да бъде \\'root\\' или \\'home\\' или \\'both\\'"

#: antix2usb.sh:340
msgid "Invalid --format option: should be ext2, ext3 or fat32"
msgstr "Невалидна --форматна опция: трябва да бъде ext2, ext3 или fat32"

#: antix2usb.sh:349
#, sh-format
msgid "Cannot install syslinux bootloader on a $PARTITION_FS partition"
msgstr "Не може да се инсталира syslinux бутлоудър на $PARTITION_FS сектор"

#: antix2usb.sh:351
#, sh-format
msgid "Cannot install extlinux bootloader on a $PARTITION_FS partition"
msgstr "Не може да се инсталира syslinux бутлоудър на $PARTITION_FS сектор"

#: antix2usb.sh:381
msgid "Invalid label, allowed characters are: A-Z a-z 0-9 - _"
msgstr "Невалидна табела, допустими символи са: A-Z a-z 0-9 - _"

#: antix2usb.sh:385
msgid "Invalid label: length exceeds 16 characters"
msgstr "Невалидна табела: дължината надхвърля 16 символа"

#: antix2usb.sh:408 antix2usb.sh:484
msgid "No USB device found"
msgstr "Не е намерено USB устройство"

#: antix2usb.sh:411
msgid "Device    Model                       Size"
msgstr "Големина устройствен модел"

#: antix2usb.sh:471
#, sh-format
msgid "Cannot access USB device $device. Sorry"
msgstr "Няма достъп до USB устройство $device. Съжелявам"

#: antix2usb.sh:515
msgid "syslinux appears not installed. Should I install it?"
msgstr "Изглежда syslinux не е инсталиран. Да го инсталирам ли?"

#: antix2usb.sh:520
msgid "Failed to install syslinux"
msgstr "Провал при инсталацията на syslinux"

#: antix2usb.sh:529
msgid "extlinux appears not installed. Should I install it?"
msgstr "Изглежда syslinux не е инсталиран. Да го инсталирам ли?"

#: antix2usb.sh:534
msgid "Failed to install extlinux"
msgstr "Провал при инсталацията на extlinux"

#: antix2usb.sh:544
#, sh-format
msgid "Invalid boot mode: \\'$BOOT_MODE\\'. Exiting"
msgstr "Невалиден бут режим: \\'$BOOT_MODE\\'. Излизане"

#: antix2usb.sh:573
msgid "ISO file doesn't fit in partition"
msgstr "ISO файла не се побира в сектора"

#: antix2usb.sh:594
#, sh-format
msgid "Estimated write speed: $speed MB/s"
msgstr "Прогнозна скорост на писане: "

#: antix2usb.sh:637
#, sh-format
msgid "$vol appears to be mounted! Unmount it, and retry. Exiting."
msgstr "$vol изглежда монтиран! Демонтирайте го, и опитайте отново. Излизане."

#: antix2usb.sh:650
#, sh-format
msgid "Preparing bootable fat32 partition on device $device"
msgstr "Подготвяне на бутващ fat32 сектор на устройство $device"

#: antix2usb.sh:655
#, sh-format
msgid "Preparing bootable ext2 partition on device $device"
msgstr "Подготвяне на бутващ fat32 сектор на устройство $device"

#: antix2usb.sh:661
#, sh-format
msgid "Preparing bootable ext3 partition on device $device"
msgstr "Подготвяне на бутващ fat32 сектор на устройство $device"

#: antix2usb.sh:667
#, sh-format
msgid "Invalid file system type: \\'$fs\\'."
msgstr "Невалиден тип файлова система: \\'$fs\\'."

#: antix2usb.sh:673
#, sh-format
msgid "USB device: $device Model: $DEVICE_INFO Size: $DEVICE_SIZE MB"
msgstr "USB устройство:  $device Модел: $DEVICE_INFO Размер: "

#: antix2usb.sh:674
#, sh-format
msgid "Ready to format $device. This will erase all data on the device."
msgstr "Готов да форматира $device. Това ще изтрие всички данни на устойството."

#: antix2usb.sh:677
msgid "Clearing partition table"
msgstr "Изчистване на секторна таблица"

#: antix2usb.sh:679
#, sh-format
msgid "Failed to zero $device"
msgstr "Провал за нулиране "

#: antix2usb.sh:686
#, sh-format
msgid "Creating bootable partition fs: ${fs}"
msgstr "Създаване на бутващ сектор фс: ${fs}"

#: antix2usb.sh:687
msgid "Running fdisk.distrib..."
msgstr "Изпълняване на fdisk.distrib..."

#: antix2usb.sh:726
#, sh-format
msgid "Failed to create partition $partition on device: $device"
msgstr "Провал при създаване на сектор $partition на устройство $device"

#: antix2usb.sh:735
#, sh-format
msgid "Creating file system on $partition1, please wait..."
msgstr "Създаване на файлова състема на $partition1, моля изчакайте..."

#: antix2usb.sh:738
#, sh-format
msgid "Failed to format $partition1"
msgstr "Провал при форматиране $partition1"

#: antix2usb.sh:740
#, sh-format
msgid "Prepared bootable partition: $partition1 label: $label1"
msgstr "Готов бутващ сектор: $partition1 табела: $label1"

#: antix2usb.sh:756
#, sh-format
msgid "Failed to mount $partition1 on $usb_mountpoint"
msgstr "Провал при монтиране на $partition1 на "

#: antix2usb.sh:761
#, sh-format
msgid "Cannot create boot directory on $usb_mountpoint"
msgstr "Не може да се създаде бутваща директория на $usb_mountpoint"

#: antix2usb.sh:763
msgid "USB device mount point ready"
msgstr "USB устройство точка за монтиране готова"

#: antix2usb.sh:773
#, sh-format
msgid "Mounting $iso_file on temporary mount point"
msgstr "Монтиране на $iso_file на временна точка за монтиране"

#: antix2usb.sh:776
#, sh-format
msgid "Failed to mount $iso_file on $iso_mountpoint"
msgstr "Провал при монтиране на $iso_file на $iso_mountpoint"

#: antix2usb.sh:779
msgid "Iso file mount point ready"
msgstr "Iso файл точка за монтиране е готова"

#: antix2usb.sh:790
#, sh-format
msgid "Unmounting $partition1 (please wait...)"
msgstr "Демонтиране на $partition1 (моля изчакайте...)"

#: antix2usb.sh:795
#, sh-format
msgid "Failed to remove USB mount point: $usb_mountpoint"
msgstr "Провал при премахване на USB точката за монтиране: $usb_mountpoint"

#: antix2usb.sh:800
#, sh-format
msgid "Failed to remove ISO mount point: $iso_mountpoint"
msgstr "Провал при премахването на ISO точка за монтиране: $iso_mountpoint"

#: antix2usb.sh:802
msgid "Mount points removed."
msgstr "Точките за монтиране са премахнати."

#: antix2usb.sh:806
msgid "Process completed."
msgstr "Процес завършен."

#: antix2usb.sh:809
msgid "Process failed"
msgstr "Процеса се провали"

#: antix2usb.sh:827 antix2usb.sh:909 antix2usb.sh:991
msgid "USB mount point undefined"
msgstr "USB точка за монтиране не е дефинирана"

#: antix2usb.sh:835
#, sh-format
msgid "Failed to create directory ${usb_mountpoint}/antiX"
msgstr "Провал при създаване на директория ${usb_mountpoint}/antiX"

#: antix2usb.sh:841
#, sh-format
msgid ""
"Copying antiX compressed file system (${ISO_FILE_SIZE}) to the mounted "
"device"
msgstr "Копиране на antiX компресирана файлова система (${ISO_FILE_SIZE}) към монтираното устройство"

#: antix2usb.sh:844
msgid "This will take a few minutes, relax..."
msgstr "Това ще отнеме няколко минути, отпуснете се..."

#: antix2usb.sh:847
#, sh-format
msgid "Failed to cp ${iso_mountpoint}/antiX"
msgstr "Провал при копиране ${iso_mountpoint}/antiX"

#: antix2usb.sh:850
msgid "Files copied to mounted device"
msgstr "Файловете са копирани към монтираното устройство"

#: antix2usb.sh:852
msgid "Copying files to boot directory"
msgstr "Копиране на файлове към бутваща директория"

#: antix2usb.sh:858
#, sh-format
msgid ""
"Failed to copy file ${iso_mountpoint}/boot/${i} to ${usb_mountpoint}/antiX"
msgstr "Провал при копиране на файл ${iso_mountpoint}/boot/${i} към ${usb_mountpoint}/antiX"

#: antix2usb.sh:866
#, sh-format
msgid "Failed to copy file ${iso_mountpoint}/${i} to ${usb_mountpoint}"
msgstr "Провал при копиране на файл ${iso_mountpoint}/${i} към ${usb_mountpoint}"

#: antix2usb.sh:874
#, sh-format
msgid ""
"Failed to copy file ${iso_mountpoint}/boot/${i} to ${usb_mountpoint}/boot"
msgstr "Провал при копиране на файл ${iso_mountpoint}/boot/${i} към ${usb_mountpoint}/boot"

#: antix2usb.sh:891 antix2usb.sh:1116
msgid "Invalid boot mode"
msgstr "Невалиден бут режим"

#: antix2usb.sh:893
msgid "Copy completed"
msgstr "Копиране завършено"

#: antix2usb.sh:915
msgid "Do you want to set up live persistence? [y|N]"
msgstr "Искате ли да настроите лайф постоянство? [y|N]"

#: antix2usb.sh:921
#, sh-format
msgid "You can set up $PRF and/or $PHF "
msgstr "Можете да настроите $PRF и/или $PHF"

#: antix2usb.sh:923
#, sh-format
msgid "Do you want to set up $PRF ? [y|N]"
msgstr "Искате ли да настоите $PRF ? [y|N]"

#: antix2usb.sh:927
#, sh-format
msgid "Set up $PRF"
msgstr "Насройване $PRF"

#: antix2usb.sh:928
msgid ""
"Type in file size. Make sure you have enough space. Just press Enter for "
"default 512 (MB) "
msgstr "Въведете файлов размер. Уверете се, че имате достатъчно място. Просто натиснете Enter за по подразбиране 512 (MB)"

#: antix2usb.sh:934 antix2usb.sh:955
msgid ""
"Type in file type: ext2, ext3 or ext4. Just press Enter for recommended "
"default ext2 "
msgstr "Въведете типа файлова система: ext2, ext3 or ext4. Просто натиснете Enter за препоръчаното по подразбиране ext2"

#: antix2usb.sh:942
#, sh-format
msgid "$PRF not set up"
msgstr "$PRF не е настроен"

#: antix2usb.sh:944
#, sh-format
msgid "Do you want to set up $PHF ? [y|N]"
msgstr "Искате ли да настроите $PHF ? [y|N]"

#: antix2usb.sh:948
#, sh-format
msgid "Set up $PHF"
msgstr "Настройване $PHF"

#: antix2usb.sh:949
msgid "Type in file size. Just press Enter for default 128 (MB)"
msgstr "Въвадете файлов размер. Просто натиснете Enter за по подразбиране 128 (MB)"

#: antix2usb.sh:963
#, sh-format
msgid "$PHF not set up"
msgstr "$PHF не е настроен"

#: antix2usb.sh:966
msgid "Finished."
msgstr "Завършено."

#: antix2usb.sh:967
#, sh-format
msgid "File(s) have been saved to $usb_mountpoint/antiX "
msgstr "Файл(овете) бяха съхранени на $usb_mountpoint/antiX"

#: antix2usb.sh:971
msgid "No persistence set up."
msgstr "Не е настроено постоянство."

#: antix2usb.sh:1019
#, sh-format
msgid "Preparing root persistence (size $CFS MB)..."
msgstr "Подготвяне на рут постоянство (размер $CFS MB)..."

#: antix2usb.sh:1029
#, sh-format
msgid "Preparing home persistence (size $CFS MB)..."
msgstr "Подготвяне на домашно постоянство ( размер $CFS MB)..."

#: antix2usb.sh:1049
msgid "Copying isolinux config files from iso"
msgstr "Копиране на isolinux конфигурационни файлове от iso"

#: antix2usb.sh:1065
msgid ""
"Copying isolinux directory and config files from iso and configuring for "
"extlinux"
msgstr "Копиране на isolinux директория и конфигурационни файлове от iso и конфигуриране за extlinux"

#: antix2usb.sh:1086
msgid ""
"Copying grub directory and config files from iso and configuring for grub"
msgstr "Копиране на grub директория и конфигурационни файлове от iso и конфигуриране за grub"

#: antix2usb.sh:1089
#, sh-format
msgid "Failed to copy grub configuaration in ${usb_mountpoint}/boot/grub"
msgstr "Провал при копиране на grub комфигурация към ${usb_mountpoint}/boot/grub"

#: antix2usb.sh:1133
msgid "Installation of boot loader syslinux failed."
msgstr "Инсталация на бутлоудър syslinux се провали."

#: antix2usb.sh:1137 antix2usb.sh:1160
msgid "Installation of master boot record failed."
msgstr "Инсталация на мастър бут запис се провали."

#: antix2usb.sh:1139
#, sh-format
msgid "syslinux successfully installed to $partition1"
msgstr "syslinux е успешно инсталиран в $partition1"

#: antix2usb.sh:1155
msgid "Installation of boot loader extlinux failed."
msgstr "Инсталация на бутлоудър extlinux се провали."

#: antix2usb.sh:1162
#, sh-format
msgid "extlinux successfully installed to $partition1"
msgstr "extlinux успешно инсталиран в $partition1"

#: antix2usb.sh:1199
#, sh-format
msgid "Installing grub on $device, please wait..."
msgstr "Инсталиране на grub на $device, моля изчакайте..."

#: antix2usb.sh:1204
#, sh-format
msgid "$GRUB_INSTALL failed, root-directory=$mountpoint, device=$device"
msgstr "$GRUB_INSTALL се провали, рут-директория=$mountpoint, устройсто=$device"

#: antix2usb.sh:1234
#, sh-format
msgid "Grub successfully installed to $device"
msgstr "Grub успешно инсталиран в $device"

#: antix2usb.sh:1301
msgid "You need to be root to run this script."
msgstr "Трябва да се рут за да изпълните този скрипт."
