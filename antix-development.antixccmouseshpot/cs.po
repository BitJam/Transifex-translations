#: antixccmouse.sh:143
msgid "Mouse Options"
msgstr "Možnosti myši "

#: antixccmouse.sh:12
msgid "Mouse Acceleration"
msgstr "Akcelerace myši "

#: antixccmouse.sh:14
msgid "Change mouse motion"
msgstr "Změna pohybu myši"

#: antixccmouse.sh:24
msgid "Acceleration (Multiplier)"
msgstr "Akcelerace"

#: antixccmouse.sh:33
msgid "Threshold (Pixels)"
msgstr "Limit (Pixely)"

#: antixccmouse.sh:42
msgid "Restore default motion"
msgstr "Tovární nastavení pohybu"

#: antixccmouse.sh:48
msgid "Button Order"
msgstr "Řád tlačítek "

#: antixccmouse.sh:51
msgid "No change"
msgstr "Beze změny "

#: antixccmouse.sh:52
msgid "Left hand layout"
msgstr "Rozvržení levé ruky "

#: antixccmouse.sh:53
msgid "Right hand layout"
msgstr "Rozvržení pravé ruky "

#: antixccmouse.sh:57
msgid "Cursor Size"
msgstr "Velikost kurzoru "

#: antixccmouse.sh:59
msgid "Change cursor size"
msgstr "Změna velikosti kurzoru"

#: antixccmouse.sh:67
msgid "Size (in pixels)"
msgstr "Velikost (v pixelech)"

#: antixccmouse.sh:76
msgid "Restore default size"
msgstr "Tovární nastavení velikosti"

#: antixccmouse.sh:82
msgid "Cursor Theme"
msgstr "Téma kurzoru "

#: antixccmouse.sh:87
msgid "After choosing a new theme, please\\n logout/login to see the changes."
msgstr "Po vybrání nového tématu, se prosím\\n\n odhlašte/přihlašte pro projevení změn. "

#: antixccmouse.sh:90
msgid "Change cursor theme"
msgstr "Změnit téma kurzoru"

#: antixccmouse.sh:116
msgid "Mouse speed changed."
msgstr "Rychlost myši změněna."

#: antixccmouse.sh:122
msgid "Using default mouse speed."
msgstr "Používám výchozí rychlost myši."

#: antixccmouse.sh:128
msgid "Now using left-hand layout."
msgstr "Nyní používám rozvržení pro levou ruku."

#: antixccmouse.sh:132
msgid "Now using right-hand layout."
msgstr "Nyní používám rozvržení pro pravou ruku."

#: antixccmouse.sh:138
msgid "Cursor size changed. Please\\nlogout/login to see changes."
msgstr "Velikost kurzoru změněna. Prosím\\n\nodhlašte se/přihlašte se pro projevení změn. "

#: antixccmouse.sh:143
msgid "Default cursor size restored.\\nPlease logout/login to see changes."
msgstr "Tovární nastavení velikosti myši obnoveno.\\n\nProsím odhlašte se/přihlašte se pro projevení změn. "
