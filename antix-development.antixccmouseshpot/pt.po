#: antixccmouse.sh:8 antixccmouse.sh:116 antixccmouse.sh:122
#: antixccmouse.sh:128 antixccmouse.sh:132 antixccmouse.sh:138
#: antixccmouse.sh:143
msgid "Mouse Options"
msgstr "Opções do mouse "

#: antixccmouse.sh:12
msgid "Mouse Acceleration"
msgstr "Aceleração"

#: antixccmouse.sh:14
msgid "Change mouse motion"
msgstr "Alterar o movimento do mouse"

#: antixccmouse.sh:24
msgid "Acceleration (Multiplier)"
msgstr "Aceleração (multiplicador)"

#: antixccmouse.sh:33
msgid "Threshold (Pixels)"
msgstr "Limite (pixel)"

#: antixccmouse.sh:42
msgid "Restore default motion"
msgstr "Restaurar o movimento padrão"

#: antixccmouse.sh:48
msgid "Button Order"
msgstr "Ordem dos Botões"

#: antixccmouse.sh:51
msgid "No change"
msgstr "sem mudança"

#: antixccmouse.sh:52
msgid "Left hand layout"
msgstr "esquema para mão esquerda"

#: antixccmouse.sh:53
msgid "Right hand layout"
msgstr "esquema para mão direita"

#: antixccmouse.sh:57
msgid "Cursor Size"
msgstr "Tamanho do Cursor"

#: antixccmouse.sh:59
msgid "Change cursor size"
msgstr "Alterar o tamanho do cursor"

#: antixccmouse.sh:67
msgid "Size (in pixels)"
msgstr "Tamanho (pixel)"

#: antixccmouse.sh:76
msgid "Restore default size"
msgstr "Restaurar o tamanho padrão"

#: antixccmouse.sh:82
msgid "Cursor Theme"
msgstr "Tema do Cursor"

#: antixccmouse.sh:87
msgid "After choosing a new theme, please\\n logout/login to see the changes."
msgstr "Após escolher um novo tema, por favor\\n\nreinicie a sessão para ver as mudanças. "

#: antixccmouse.sh:90
msgid "Change cursor theme"
msgstr "Alterar o tema do cursor"

#: antixccmouse.sh:116
msgid "Mouse speed changed."
msgstr "Alterada a velocidade do mouse."

#: antixccmouse.sh:122
msgid "Using default mouse speed."
msgstr "Usando a velocidade padrão do mouse."

#: antixccmouse.sh:128
msgid "Now using left-hand layout."
msgstr "Agora usando esquema para a mão esquerda."

#: antixccmouse.sh:132
msgid "Now using right-hand layout."
msgstr "Agora usando esquema para a mão direita."

#: antixccmouse.sh:138
msgid "Cursor size changed. Please\\nlogout/login to see changes."
msgstr "Alterado o tamanho do cursor. Por favor,\\n\nreinicie a sessão para ver a mudança. "

#: antixccmouse.sh:143
msgid "Default cursor size restored.\\nPlease logout/login to see changes."
msgstr "Restaurado o tamanho padrão do cursor. Por\\n\nfavor, reinicie a sessão para ver a mudança. "
