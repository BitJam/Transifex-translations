#: antixccmouse.sh:8 antixccmouse.sh:116 antixccmouse.sh:122
#: antixccmouse.sh:128 antixccmouse.sh:132 antixccmouse.sh:138
#: antixccmouse.sh:143
msgid "Mouse Options"
msgstr "Oпції миші "

#: antixccmouse.sh:12
msgid "Mouse Acceleration"
msgstr "Прискорення миші"

#: antixccmouse.sh:14
msgid "Change mouse motion"
msgstr "Змінити рухи миші"

#: antixccmouse.sh:24
msgid "Acceleration (Multiplier)"
msgstr "Прискорення (коефіцієнт)"

#: antixccmouse.sh:33
msgid "Threshold (Pixels)"
msgstr "Поріг (пікселі)"

#: antixccmouse.sh:42
msgid "Restore default motion"
msgstr "Відновити типові рухи"

#: antixccmouse.sh:48
msgid "Button Order"
msgstr "Послідовність кнопок"

#: antixccmouse.sh:51
msgid "No change"
msgstr "Без змін"

#: antixccmouse.sh:52
msgid "Left hand layout"
msgstr "Під ліву руку"

#: antixccmouse.sh:53
msgid "Right hand layout"
msgstr "Під праву руку"

#: antixccmouse.sh:57
msgid "Cursor Size"
msgstr "Розмір вказівника"

#: antixccmouse.sh:59
msgid "Change cursor size"
msgstr "Змінити розмір вказівника"

#: antixccmouse.sh:67
msgid "Size (in pixels)"
msgstr "Розмір (в пікселях)"

#: antixccmouse.sh:76
msgid "Restore default size"
msgstr "Відновити типовий розмір"

#: antixccmouse.sh:82
msgid "Cursor Theme"
msgstr "Тема вказівника"

#: antixccmouse.sh:87
msgid "After choosing a new theme, please\\n logout/login to see the changes."
msgstr "Після вибору нової теми, вийдіть\\n\n та зайдіть, щоб побачити зміни. "

#: antixccmouse.sh:90
msgid "Change cursor theme"
msgstr "Змінити тему вказівника"

#: antixccmouse.sh:116
msgid "Mouse speed changed."
msgstr "Швидкість миші змінено."

#: antixccmouse.sh:122
msgid "Using default mouse speed."
msgstr "Використовувати типову швидкість миші."

#: antixccmouse.sh:128
msgid "Now using left-hand layout."
msgstr "Використовувати під ліву руку."

#: antixccmouse.sh:132
msgid "Now using right-hand layout."
msgstr "Використовувати під праву руку."

#: antixccmouse.sh:138
msgid "Cursor size changed. Please\\nlogout/login to see changes."
msgstr "Розмір вказівника змінено. Вийдіть\\n\nта зайдіть знову, щоб побачити зміни. "

#: antixccmouse.sh:143
msgid "Default cursor size restored.\\nPlease logout/login to see changes."
msgstr "Типовий розмір вказівника відновлено.\\n\nВийдіть та зайдіть знову, щоб побачити зміни. "
