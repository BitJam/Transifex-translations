msgid "Mouse Options"
msgstr "Mausoptionen "

#: antixccmouse.sh:12
msgid "Mouse Acceleration"
msgstr "Mausbeschleunigung"

#: antixccmouse.sh:14
msgid "Change mouse motion"
msgstr "Ändere Mausbewegung"

#: antixccmouse.sh:24
msgid "Acceleration (Multiplier)"
msgstr "Beschleunigung (Multiplikator)"

#: antixccmouse.sh:33
msgid "Threshold (Pixels)"
msgstr "Schwellenwert (in Pixel)"

#: antixccmouse.sh:42
msgid "Restore default motion"
msgstr "Standardbewegung wiederherstellenn"

#: antixccmouse.sh:48
msgid "Button Order"
msgstr "Tastenreihenfolge"

#: antixccmouse.sh:51
msgid "No change"
msgstr "Keine Änderung"

#: antixccmouse.sh:52
msgid "Left hand layout"
msgstr "linkshändiges Layout"

#: antixccmouse.sh:53
msgid "Right hand layout"
msgstr "rechtshändiges Layout"

#: antixccmouse.sh:57
msgid "Cursor Size"
msgstr "Zeigergröße"

#: antixccmouse.sh:59
msgid "Change cursor size"
msgstr "Ändere Mauszeigergröße"

#: antixccmouse.sh:67
msgid "Size (in pixels)"
msgstr "Größe (in Pixel)"

#: antixccmouse.sh:76
msgid "Restore default size"
msgstr "Standardgröße wiederherstellen"

#: antixccmouse.sh:82
msgid "Cursor Theme"
msgstr "Zeigerthema"

#: antixccmouse.sh:87
msgid "After choosing a new theme, please\\n logout/login to see the changes."
msgstr "Nach Themaauswahl, bitte\\n\n neueinloggen um Änderungen wirksam zumachen. "

#: antixccmouse.sh:90
msgid "Change cursor theme"
msgstr "Ändere Mauszeigerthema"

#: antixccmouse.sh:116
msgid "Mouse speed changed."
msgstr "Mausgeschwindgikeit geändert."

#: antixccmouse.sh:122
msgid "Using default mouse speed."
msgstr "Verwende Standard-Mausgeschwndigkeit."

#: antixccmouse.sh:128
msgid "Now using left-hand layout."
msgstr "Verwende linkshändiges Layout."

#: antixccmouse.sh:132
msgid "Now using right-hand layout."
msgstr "Verwende rechtshändiges Layout."

#: antixccmouse.sh:138
msgid "Cursor size changed. Please\\nlogout/login to see changes."
msgstr "Mauszeigergröße geändert. Bitte\\n\nneueinloggen um Änderungen wirksam zumachen. "

#: antixccmouse.sh:143
msgid "Default cursor size restored.\\nPlease logout/login to see changes."
msgstr "Standard Mauszeigergröße wiederhergestellt.\\n\nBitte neueinloggen, um Änderungen wirksam zumachen. "
