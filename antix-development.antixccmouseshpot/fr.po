msgid "Mouse Options"
msgstr "Options de la souris"

#: antixccmouse.sh:12
msgid "Mouse Acceleration"
msgstr "Accélération de la souris"

#: antixccmouse.sh:14
msgid "Change mouse motion"
msgstr "Changer les mouvements de la souris"

#: antixccmouse.sh:24
msgid "Acceleration (Multiplier)"
msgstr "Accélération (multiplicateur)"

#: antixccmouse.sh:33
msgid "Threshold (Pixels)"
msgstr "Seuil (Pixels)"

#: antixccmouse.sh:42
msgid "Restore default motion"
msgstr "Restaurer le mouvement par défaut"

#: antixccmouse.sh:48
msgid "Button Order"
msgstr "Ordre des boutons"

#: antixccmouse.sh:51
msgid "No change"
msgstr "Pas de changement"

#: antixccmouse.sh:52
msgid "Left hand layout"
msgstr "Mise en page de gauche"

#: antixccmouse.sh:53
msgid "Right hand layout"
msgstr "Mise en page de droite"

#: antixccmouse.sh:57
msgid "Cursor Size"
msgstr "Taille du curseur"

#: antixccmouse.sh:59
msgid "Change cursor size"
msgstr "Changer la taille du curseur"

#: antixccmouse.sh:67
msgid "Size (in pixels)"
msgstr "Taille (en pixels)"

#: antixccmouse.sh:76
msgid "Restore default size"
msgstr "Restaurer la taille par défaut"

#: antixccmouse.sh:82
msgid "Cursor Theme"
msgstr "Thème de curseur"

#: antixccmouse.sh:87
msgid "After choosing a new theme, please\\n logout/login to see the changes."
msgstr "Après avoir choisi un nouveau thème, Veuillez\\n fermer la session/ouvrir la session pour voir les changements."

#: antixccmouse.sh:90
msgid "Change cursor theme"
msgstr "Thème de curseur"

#: antixccmouse.sh:116
msgid "Mouse speed changed."
msgstr "La vitesse de la souris a changé."

#: antixccmouse.sh:122
msgid "Using default mouse speed."
msgstr "En utilisant la vitesse de la souris par défaut."

#: antixccmouse.sh:128
msgid "Now using left-hand layout."
msgstr "Maintenant, en utilisant la mise en page de gauche."

#: antixccmouse.sh:132
msgid "Now using right-hand layout."
msgstr "Maintenant, en utilisant la mise en page de droite."

#: antixccmouse.sh:138
msgid "Cursor size changed. Please\\nlogout/login to see changes."
msgstr "La taille du curseur a changé. Veuillez\\n fermer la session/ouvrir la session pour voir les changements."

#: antixccmouse.sh:143
msgid "Default cursor size restored.\\nPlease logout/login to see changes."
msgstr "La taille du curseur par défaut a été restaurée.\\nVeuillez fermer la session/ouvrir la session pour voir les changements."
