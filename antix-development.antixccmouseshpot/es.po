#: antixccmouse.sh:143
msgid "Mouse Options"
msgstr "Opciones del ratón "

#: antixccmouse.sh:12
msgid "Mouse Acceleration"
msgstr "Aceleración del Mouse"

#: antixccmouse.sh:14
msgid "Change mouse motion"
msgstr "Cambiar el movimiento del ratón"

#: antixccmouse.sh:24
msgid "Acceleration (Multiplier)"
msgstr "Aceleración (Multiplicador)"

#: antixccmouse.sh:33
msgid "Threshold (Pixels)"
msgstr "Umbral (Pixeles)"

#: antixccmouse.sh:42
msgid "Restore default motion"
msgstr "Restaurar movimiento predeterminado"

#: antixccmouse.sh:48
msgid "Button Order"
msgstr "Orden de los Botones"

#: antixccmouse.sh:51
msgid "No change"
msgstr "Sin Cambios"

#: antixccmouse.sh:52
msgid "Left hand layout"
msgstr "Disposición para zurdos"

#: antixccmouse.sh:53
msgid "Right hand layout"
msgstr "Disposición para diestros"

#: antixccmouse.sh:57
msgid "Cursor Size"
msgstr "Tamaño del Cursor"

#: antixccmouse.sh:59
msgid "Change cursor size"
msgstr "Cambiar el tamaño del cursor"

#: antixccmouse.sh:67
msgid "Size (in pixels)"
msgstr "Tamaño (en pixeles)"

#: antixccmouse.sh:76
msgid "Restore default size"
msgstr "Restaurar tamaño predeterminado"

#: antixccmouse.sh:82
msgid "Cursor Theme"
msgstr "Tema del Cursor"

#: antixccmouse.sh:87
msgid "After choosing a new theme, please\\n logout/login to see the changes."
msgstr "Después de elegir un nuevo tema, por favor\\n\n cierre/inicie sesión para ver los cambios. "

#: antixccmouse.sh:90
msgid "Change cursor theme"
msgstr "Cambiar el tema del cursor"

#: antixccmouse.sh:116
msgid "Mouse speed changed."
msgstr "Velocidad del mouse cambiada."

#: antixccmouse.sh:122
msgid "Using default mouse speed."
msgstr "Usando la velocidad predeterminada del mouse."

#: antixccmouse.sh:128
msgid "Now using left-hand layout."
msgstr "Usando distribución para zurdos"

#: antixccmouse.sh:132
msgid "Now using right-hand layout."
msgstr "Usando distribución para diestros"

#: antixccmouse.sh:138
msgid "Cursor size changed. Please\\nlogout/login to see changes."
msgstr "Tamaño del cursor cambiado, por favor\\n\n cierre/inicie sesión para ver los cambios. "

#: antixccmouse.sh:143
msgid "Default cursor size restored.\\nPlease logout/login to see changes."
msgstr "Tamaño predeterminado del cursor restaurado, por favor\\n\n cierre/inicie sesión para ver los cambios. "
