#. What thing we are copying or cloning from: file, device or directory
msgid "file"
msgstr "ፋይል"

#. What thing we are copying or cloning from: file, device or directory
msgid "device"
msgstr "አካል "

#. Cloning only copies certain files to make a brand-new live-usb
msgid "cloning"
msgstr "ማባዣ"

#. What thing we are copying or cloning from: file, device or directory
msgid "directory"
msgstr "ዳይሬክቶሪ "

