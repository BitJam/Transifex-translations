msgstr "ማስጀመሪያ"

msgid "Skip Error: %s"
msgstr "ስህተት መዝለያ: %s"

msgid "power off"
msgstr "ሐይል ጠፍቷል"

msgid "reboot"
msgstr "ዳግም ማስጀመሪያ"

msgid "Unknown command"
msgstr "ያልታወቀ ትእዛዝ: "

msgid "Please try again"
msgstr "እባክዎን እንደገና ይሞክሩ"

msgid "Unmount successful"
msgstr "ማውረዱ ተሳክቷል"

msgid "Press %s to %s"
msgstr "ይጫኑ %s ለ %s"

msgid "Will now %s the system"
msgstr "አሁን %s ስርአቱ"

msgid "Fatal Error"
msgstr "አደገኛ ስህተት"

msgid "Non-Fatal Error"
msgstr "አደገኛ ያልሆነ ስህተት"

msgid "continue"
msgstr "ይቀጥሉ"

msgid "yes"
msgstr "አዎ"

msgid "no"
msgstr "አይ"

